    background:#F3F2F0;
    border-radius:5px;
    position:relative;
    overflow: hidden;
    display:inline;
}
ul.course_list li.nobg{
    box-shadow: none;
    background:none;
}
ul.course_list li.nobg img {
    margin:0;
}
ul.course_list li div.pic {
    float:left;
    padding: 11px 0 5px 11px;
}
ul.course_list li div.pic img{
    width:210px;
    height:140px;
    cursor:pointer;
}
._ptitle .sortby{
    background:url("../Images/sortDown.png") no-repeat;
    cursor:pointer;
}
._ptitle .on{
    background:url("../Images/sortUp.png") no-repeat;
}
._ptitle .hw_num{
    background-position:45px 26px;
}
._ptitle .hw_time{
    background-position:103px 26px;
}
.course_cover {
