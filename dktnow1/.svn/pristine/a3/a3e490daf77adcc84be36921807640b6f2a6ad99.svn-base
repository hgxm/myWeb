package dkt.teacher.model;

public class Homework {
	
	private String Id; // 
	private String actId; // 作业id
	private String toId;  // 题目id
	private String toType; // 题目类型
	private String toPath;  // 题目图片地址
	private String toAnswer;  // 正确答案
	private String answer;  // 学生答案
	private byte[] toBitmap; // 题目二进制图片
	private String toOption;
	private String toTitle;
	private String toTableName; // 主观题答案表
	private byte[] toAnswerBitmap;
	private String subId;
	private String type;
	private byte[] toTeacherBitmap; // 老师批改最终图
	
	
	public void setId(String id) {
		Id = id;
	}
	public String getId() {
		return Id;
	}
	public void setActId(String actId) {
		this.actId = actId;
	}
	public String getActId() {
		return actId;
	}
	public void setToId(String toId) {
		this.toId = toId;
	}
	public String getToId() {
		return toId;
	}
	public void setToType(String toType) {
		this.toType = toType;
	}
	public String getToType() {
		return toType;
	}
	public void setToPath(String toPath) {
		this.toPath = toPath;
	}
	public String getToPath() {
		return toPath;
	}
	public void setToAnswer(String toAnswer) {
		this.toAnswer = toAnswer;
	}
	public String getToAnswer() {
		return toAnswer;
	}
	public void setAnswer(String answer) {
		this.answer = answer;
	}
	public String getAnswer() {
		return answer;
	}
	public void setToBitmap(byte[] toBitmap) {
		this.toBitmap = toBitmap;
	}
	public byte[] getToBitmap() {
		return toBitmap;
	}
	public void setToTableName(String toTableName) {
		this.toTableName = toTableName;
	}
	public String getToTableName() {
		return toTableName;
	}
	public void setToAnswerBitmap(byte[] toAnswerBitmap) {
		this.toAnswerBitmap = toAnswerBitmap;
	}
	public byte[] getToAnswerBitmap() {
		return toAnswerBitmap;
	}
	public void setToOption(String toOption) {
		this.toOption = toOption;
	}
	public String getToOption() {
		return toOption;
	}
	public void setToTitle(String toTitle) {
		this.toTitle = toTitle;
	}
	public String getToTitle() {
		return toTitle;
	}
	public void setSubId(String subId) {
		this.subId = subId;
	}
	public String getSubId() {
		return subId;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getType() {
		return type;
	}
	public void setToTeacherBitmap(byte[] toTeacherBitmap) {
		this.toTeacherBitmap = toTeacherBitmap;
	}
	public byte[] getToTeacherBitmap() {
		return toTeacherBitmap;
	}
}
