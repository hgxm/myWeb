    text-align:center;
    cursor:pointer;
}
.rig_edit label.edit_leaguer:hover{
    background-position:-16px -32px;
}
.rig_cure{
    float:left;
    width:350px;
}
.rig_cure label,.rig_cure div{
    float:left;
    font-size:14px;
}
.rig_cure label{
    display:block;
    margin:7px 0px;
    height:25px;
    line-height:25px;
    white-space:nowrap;
}
.rig_cure div span{
    float:left;
    display:inline;
    height:30px;
    line-height:30px;
    padding:0px 5px;
    margin:7px 5px 0px 5px;
    border:1px solid #0f8740;
    cursor:pointer;
    white-space:nowrap;
}
.rig_cure div.ruge_on span{
    position:relative;
}
.rig_cure div{
    float:left;
    width:320px;
    max-height:250px;
    overflow:auto;
}
.rig_cure div.all {
    width:450px;
}
.rig_cure div.ruge_none span.on{
    float:left;
    background:#0f8740;
    color:#fff;
}
.flex_close{
    display:none;
    position:absolute;
    right:-8px;
    top:-8px;
