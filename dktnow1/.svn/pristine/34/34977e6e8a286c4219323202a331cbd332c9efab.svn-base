/*课程表*/
.sy_courses{
    float:left;
}
.sy_courses li{
    float:left;
    width:100%;
    height:53px;
    border-bottom:1px solid #dbdbdb;
}
.sy_courses li label,.sy_courses li p{
    display:inline;
    float:left;
    height:53px;
    line-height:53px;
    font-size:18px;
    text-align:center;
}
.sy_courses li p{
    position:relative;
    width:95px;
    border-left:1px solid #ecc2c2;
    cursor:pointer;
    overflow:hidden;
}
.sy_courses li label{
    width:94px;
    color:#a0a0a0;
}
.sy_courses li.sy_header{
    border-bottom:1px solid #ecc2c2;
}
.sy_courses li.sy_header p{
    width:96px;
    border-left:none;
    color:#d65858;
    cursor:default;
}
.group_content .hon_stu{
    float:left;
    _width:600px;
}
.syll_main{
    _height:565px;
}
.syl_cour{
    width:450px;
}
.syl_cour label,.syl_cour div,.syl_cour div span{
    display:block;
    float:left;
    line-height:25px;
    color:#3e3e3e;
}
.syl_cour label{
    width:75px;
    height:25px;
    margin-top:5px;
    text-align:right;
    font-weight:bold;
}
.syl_cour div{
    width:460px;
}
.syl_cour div span{
    height:23px;
    margin:5px 0px 0px 10px;
    padding:0px 15px;
    border:1px solid #3aba6e;
    cursor:pointer;
}
.syl_cour div .syl_num{
    background:#3aba6e;
    color:#fff;
}
/*侯祯怿 5-21*/
.foot_bot,.foot_top{
    width:1000px;
    border-bottom:1px solid #b2b2b2;
    margin:0 auto;
    margin-top:10px;
    font-size:0px;
    line-height:0px;
}
.foot_top{
    margin-top:0px;
    border-bottom:1px solid #e8e8e8;
}
/*班级管理页面更改 5-22*/
.warp .me_re li{
    font-weight:500;
}
.warp .me_re li.tab_return{
    float:right;
}
.top_tab_menu li.tab_return{
    float:right;
}
.top_tab_menu li.tab_return:hover {
    color:#22a558;
}
#notrend {
    width:770px;
    text-align:center;
    margin-top:10px;
}
#notrend a{
    line-height:50px;
    font-size:18px;
    color: #3E3E3E;
}
.font{
    width:100%;
    text-align:center;
    line-height:50px;
    font-size:18px;
}
/*标题去加粗 返回 5-23*/
.top_tab_menu li.selected, .top_tab_menu li.tab_return, .top_tab_menu li.tab_name{
    font-weight:400;
}
.top_tab_menu li.tab_return,{
    margin-right:30px;
    color:#040404;
    font-size:18px;
}
.honor_among li.honor_click span{
    font-weight:400;
}
.notice_over .font,.stu_crere .font{
    color: #3E3E3E;
    font-size: 18px;
}
/*信息管理*/
.relea_cen{
    float:left;
    width:700px;
}
.release_page{
    float:right;
    margin:10px 20px 20px 0px;
}
.release_page a{
    display:block;
    float:left;
    padding:5px 10px;
    margin-left:5px;
    border:1px solid #20D691;
    cursor:pointer;
}
.release_page a:hover{
    background:#93E5BD;
}
