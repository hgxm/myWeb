.auto_issue {
    width: 100%;
    height: 80px;
}
.auto_issue label {
    line-height:80px;
    float: left;
    width: 120px;
    background: #F7F7F6;
    text-align: center;
    font-size: 18px;
    color: #313030;
}
.auto_issue div {
    float: left;
    display: inline;
    height:80px;
}
.auto_issue div.ts {
    float:left;
    display:block;
    height:80px;
    line-height:80px;
    color:#979797;
    margin-left:15px;
}
.auto_issue div.ts img {
    position:relative;
    top:5px;
    right:3px;
}
.auto_issue div .switchOn ,.auto_issue div .switchOff{
    float:left;
    width:85px;
    height:31px;
    margin-top:25px;
    margin-left:15px;
    border-radius:20px;
    position:relative;
    cursor:pointer;
}
.auto_issue div .switchOn {
    background:#499669;
}
.auto_issue div .switchOff {
    background:#cbd3d6 ;
}
.auto_issue div .switchOn span {
    position:absolute;
    top: 5px;
    left: 30px;
    font-size:14px;
}
.auto_issue div .switchOff span {
    position:absolute;
    top: 5px;
    left: 40px;
    font-size:14px;
}
.auto_issue div .switchOn span {
    color:#abd3bc;
}
.auto_issue div .switchOff span {
    color: #94a0a5;
}
.auto_issue div .switchOn label {
    position:absolute;
    right:4px;
    top:3px;
    width:25px;
    height:25px;
    border-radius:15px;
    cursor:pointer;
}
.auto_issue div .switchOff label {
    position:absolute;
    left:4px;
    top:3px;
    width:25px;
    height:25px;
    border-radius:15px;
    cursor:pointer;
}
.auto_issue div .switchOn label {
    background:#abd3bc;
}
.auto_issue div .switchOff label {
    background:#94a0a5;
}
.hw_hrcontent {
    height:auto;
    background:#fff;
}
.hw_hrcontent label{
    font-weight: 400;
    height:448px;
}
.hw_hrcontent #content{
    margin-left:12px;
}
