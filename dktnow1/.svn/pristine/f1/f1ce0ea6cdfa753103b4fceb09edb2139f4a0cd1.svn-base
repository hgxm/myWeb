/* dialog 弹出窗口 */
/*--->导出课程*/
#export_course p {
    padding:10px 5px 5px 5px;
    color:#292929;
    font-size:14px;
    font-family: "Hiragino Sans GB","Hiragino Sans GB W3","Microsoft YaHei","微软雅黑",tahoma,arial,simhei,"黑体";
}
#export_course p.export_num input {
    margin-top:6px;
}
#export_course p input {
    margin-left:5px;
}
#export_course p:last-child {
    width:320px;
    padding:0 20px;
    color:red;
}
/*--->分享课程*/
#share_course p:last-child {
    float:left;
    position:relative;
}
#share_course .share_to {
    position:absolute;
    top:78px;
    left:25px;
    display:block;
    color:#435e99;
    font-size:16px;
    font-weight: 400;;
}
#share_course ul.s_c_tabmenu {
    height: 40px;
    white-space: nowrap;
    margin-left:85px;
    margin-top:10px;
}
#share_course ul.s_c_tabmenu li {
    float:left;
    width:90px;
    height:40px;
    line-height:40px;
    color:#9e9ea2;
    font-size:14px;
    text-align:center;
    cursor:pointer;
}
#share_course ul.s_c_tabmenu li.current  {
    color: #282928;
    font-weight: 400;;
    background: url('../Images/d_selected.png') no-repeat bottom;
}
#share_course div.s_c_tabbox {
    float: left;
    width: 535px;
    height: auto;
    min-height: 80px;
    background: #F5F5F5;
    border:1px solid #CAC6C1;
    overflow: hidden;
    margin-left:10px;
}
#share_course p {
    font-size:14px;
    color:#292929;
    margin-left:10px;
}
#share_course p input {
    margin-top:10px;
}
#share_course .s_c_tabbox ul li {
