/* 仪表盘 */
.panel {
    width:840px;
    height:auto;
    margin-top:25px;
    margin-left:28px;
}
.panel .panel_tab_menu {
    float: left;
    width: 100%;
    height: auto;
    overflow: hidden;
}
.panel .panel_tab_menu li {
    float: left;
    width: 140px;
    height: 50px;
    line-height: 50px;
    margin-right: 20px;
    font-size: 18px;
    color: #99999D;
    text-align: center;
    cursor: pointer;
}
.panel .panel_tab_menu li.current {
    font-weight: 400;;
    color: #080808;
}
.stat_box {
    float:left;
    width:408px;
    height:auto;
    margin-right:12px;
    margin-top:12px;
    padding-bottom:25px;
    background:#fff;
    box-shadow: 0 0 2px 2px rgba(0, 0, 0, 0.1);
    display:inline;
    overflow:hidden;
}
.stat_box p {
    height:46px;
    line-height:46px;
    border-bottom:1px solid #E0DDD9;
    padding:0 20px;
    overflow:hidden;
    font-size:14px;
    color:#5d5d5d;
}
.stat_box .diagram {
    height:240px;
    border-bottom:1px dashed #C9C5C0;
    margin: 0 20px;
}
.stat_box .diagram.one {
    margin:0 20px;
}
.stat_box .diagram ul {
    float:left;
}
.stat_box .diagram.one ul.oneul {
    margin:40px 0 0 10px;
    _margin:40px 0 0 0;
}
.stat_box .diagram.one ul li {
    float:left;
    width:52px;
    height:auto;
    margin:0 15px;
}
.stat_box .diagram.one ul li span {
    display:inline;
    float:left;
    width:52px;
    height:140px;
    background:#ECEBE9;
    position:relative;
}
.stat_box .diagram ul li label {
    display:block;
    float:left;
    text-align:center;
    font-size:13px;
    color:#080808;
    padding:10px 0;
    white-space:nowrap;
    overflow:hidden;
}
.stat_box .diagram.one ul li label {
    width:52px;
}
.stat_box .diagram.three ul li label {
    width:72px;
}
.diagram ul li span.chinese cite,.math cite,.english cite,.other cite {
    float:left;
    width:100%;
    background:#F6C748;
    position:absolute;
    bottom:0px;
    left:0px;
}
.diagram.one.resource ul li span cite {
    background:#C186E6;
}
.diagram span cite i{
    position:absolute;
    top:-20px;
    text-align:center;
    color:#5d5d5d;
    margin:0 auto;
    width:52px;
}
.stat_box .ranking p{
    height:50px;
    line-height:50px;
    _line-height: 50px;
    border-bottom:none;
}
.stat_box .ranking.one ul,.stat_box .ranking.two ul {
    display:inline;
    float:left;
    margin-top:10px;
    margin-left:20px;
    _margin-left:10px;
}
.stat_box .ranking ul li {
    float:left;
    width:380px;
    color:#888580;
    font-size:14px;
    display:inline;
    margin-top:5px;
}
.stat_box .ranking ul li label {
    float:left;
}
.stat_box .ranking.one ul li label{
    width:30px;
}
.stat_box .ranking.two ul li label{
    width:50px;
}
.stat_box .ranking ul li label.count{
    width:75px;
}
.stat_box .ranking ul li span {
    float:left;
    width:240px;
    height:8px;
    margin:6px 10px 0 5px;
    background:#FFF4F3;
    position:relative;
    overflow:hidden;
}
.ranking ul li span i {
    float: left;
    height:8px;
    position:absolute;
    left:0;
    background:#FF6769;
    display:inline;
    overflow:hidden;
}
.ranking ul li label cite {
    color:#FE3233;
}
.stat_box .diagram.two ul.twoul {
    margin:30px 0 0 0;
    _margin:30px 0 0 0;
}
.diagram.two ul li {
    float:left;
    width:140px;
    height:25px;
    margin:0 35px 15px 5px;
    font-size:14px;
    color:#76746F;
    overflow:hidden;
    display:inline;
}
.diagram.two span.serialNum_good,.diagram.two span.serialNum {
    float: left;
    width: 22px;
    height: 22px;
    line-height: 22px;
    text-align:center;
    color:#fff;
    margin-right:10px;
}
.diagram.two span.serialNum_good {
    background: url('/apps/resource/Public/Images/Ilc/serialNum_good.png') no-repeat;
}
.diagram.two span.serialNum {
    background: url('/apps/resource/Public/Images/Ilc/serialNum.png') no-repeat;
}
.stat_box .diagram.three {
    margin: 0 20px;
}
.stat_box .diagram.three, .stat_box .diagram.four {
    height: 162px;
    border-bottom: 1px dashed #C9C5C0;
}
.stat_box .diagram.three ul {
    margin: 30px 0 0 15px;
    _margin: 30px 0 0 0;
}
.stat_box .diagram.three ul li {
    float: left;
    width: 72px;
    height: auto;
    margin: 0 6px;
    _margin: 0 10px;
}
.stat_box .diagram.three ul li span {
    display: block;
    float: left;
    width: 52px;
    height: 140px;
    background: #ECEBE9;
    position: relative;
    margin-left:10px;
}
.diagram.three ul li span.three_res cite, .upload_res cite, .other_res cite {
    float: left;
    width: 100%;
    background: #C186E6;
    position: absolute;
    bottom: 0px;
    left: 0px;
}
.ranking3 ul ,.ranking4 ul{
    margin:10px 0 0 20px;
}
.ranking3 ul li,.ranking4 ul li {
    float: left;
    width: 140px;
    height: 25px;
    margin: 0 35px 6px 5px;
    font-size: 14px;
    color: #76746F;
    overflow: hidden;
}
.ranking3 ul li span ,.ranking4 ul li span{
    width: 0;
    margin-right:5px;
}
.ranking3 p ,.ranking4 p{
    border-bottom:none;
}
.diagram.four ul {
    margin-top:10px;
}
.diagram.four ul li {
    float: left;
    width: 140px;
    height: 25px;
    margin: 0 35px 5px 5px;
    font-size: 14px;
    color: #76746F;
    overflow: hidden;
}
.diagram.four ul li span {
    width: 0;
    margin-right:5px;
}
.ranking4 {
    height: 190px;
    float:left;
    border-bottom: 1px dashed #C9C5C0;
}