/* 拓展阅读--资源详情 */
/* 未转码资源--下载 */
.res_right {
    width: 750px;
    height:auto;
    border-radius: 2px;
    color: #363636;
    margin-top:25px;
    margin-left:150px;
    border: 1px solid lightGrey;
    background: #F2F2F2;
    overflow: hidden;
    font-size:16px;
}
.res_right .back {
    float:right;
    color: #363636;
    margin-top:10px;
    margin-right:20px;
    letter-spacing: 4px;
    cursor:pointer;
}
.res_right .back:hover {
    color:#22a558;
}
.res_right .res_info {
    float:left;
    width:700px;
    margin:10px 25px;
}
.res_right .res_info li {
    float:left;
    line-height: 30px;
    margin-bottom:10px;
}
.res_right  .libox li {
    width:700px;
}
.res_right .res_info li label {
    float:left;
    width: 90px;
    letter-spacing: 2px;
    float: left;
    padding-right: 10px;
    line-height: 30px;
    text-align: right;
    color: #494949;
}
.res_right .res_info li div.title_box {
    float:left;
}
.res_right .res_info li div input {
    float:left;
    width:180px;
    height:25px;
}
.res_right .res_info li .show_box {
    float:left;
    display:inline;
}
.res_right .res_info .msg {
    float:left;
}
.res_right .res_info li div a:hover {
    color:#22a558;
}
.res_right .res_info li.coBtn {
    float:left;
    margin-top:20px;
    margin-left:250px;
}
.res_right .res_info li.coBtn .save {
    width: 174px;
    height: 38px;
    background: url("../Images/btns.png") repeat scroll 0 0;
    color: white;
    border: medium none;
    cursor: pointer;
    font-size: 14px;
}
/* 视频文件预览 */
#video {
    float: left;
    width:700px;
    height:408px;
    position:relative;
    z-index: 100;
    margin-bottom:10px;
}
/* 文档文件预览 */
#documentViewer {
    width:700px;
    height:500px;
    margin-bottom:10px;
}
.document .la_bar ,.video .la_bar {
    float:left;
}