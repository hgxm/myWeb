/* 创建作业 */
.create_homework {
    width:100%;
    height:auto;
    overflow:hidden;
    background:#fff;
    margin-top:25px;
}
.create_homework .title {
    height:90px;
    line-height:90px;
    border-bottom:1px solid #ECEBE9;
    font-size:24px;
    color:#313030;
}
.create_homework .title span {
    float:left;
    margin-left:20px;
}
.create_homework .title a {
    float:right;
    margin-right:45px;
}
.create_homework .title a:hover {
    color:#22a558;
}
.hw_title {
    width:100%;
    height:80px;
    line-height:80px;
}
.create_homework div.hw_title label,div.hw_hrcontent label,.hw_require label, .hw_point label,.hw_group label,.hw_addTopic label {
    float:left;
    width:120px;
    background:#F7F7F6;
    text-align:center;
    font-size:18px;
    color:#313030;
    font-weight:bold;
}
div.hw_hrcontent label{
    font-weight:normal;
}
.hw_title div {
    float:left;
    display:inline;
}
.hw_title div input {
    width:400px;
    height:35px;
    margin-left:15px;
    text-indent:5px;
}
.hw_title div span {
    margin-left:15px;
    font-size:14px;
    color:#8e8e8e;
}
.hw_require {
    width:100%;
    line-height:30px;
    padding-bottom:15px;
}
.hw_require div {
    float:left;
    display:inline;
    margin-top: 20px;
}
.hw_require div textarea {
    width:795px;
    height:90px;
    margin-left: 15px;
    text-indent:5px;
}
.hw_point {
    width:100%;
    height:110px;
    line-height:110px;
}
.hw_point div {
    float: left;
    width:800px;
    height:120px;
    margin-left:15px;
    position:relative;
    margin-top:10px;
}
.hw_point div.bar {
    float:left;
    width:100%;
    height:40px;
    margin-top:30px;
}
.hw_point div .limit {
    float:left;
    height:40px;
    margin-left:15px;
}
.hw_point div .limit li {
    width:100%;
    float:left;
    position:relative;
    top:-5px;
}
.hw_point div .limit input {
    margin-right:10px;
}
.hw_point div cite {
    float:left;
    line-height: 30px;
}
.hw_point div p {
    display:inline;
    float: left;
    width:370px;
    height:3px;
    margin:13px 5px 0 5px;
    background:#E5E3E0;
    line-height:3px;
    position:relative;
}
.hw_point div p span {
    float:left;
    width:30%;
    height:3px;
    background:#EC5653;
    position:relative;
}
.hw_point div p span i.point {
    position:absolute;
    top:-10px;
    right:-15px;
}
.hw_point div p span i.dir {
    position:absolute;
    bottom:-13px;
    right:-10px;
}
.hw_group {
    width:100%;
    height:80px;
    line-height:80px;
}
.hw_group label input {
    margin-right:5px;
}
.hw_group div {
    float: left;
    margin-left:15px;
}
.hw_group .choose_group {
    padding:5px 10px;
    border:1px solid #20D691;
    cursor:pointer;
}
.hw_addTopic {
    width:100%;
    height:80px;
    line-height:80px;
}
.hw_addTopic div {
    float: left;
    margin-left:15px;
}
.hw_addTopic span {
    padding:5px 10px;
    border:1px solid #20D691;
    margin-right:10px;
    cursor:pointer;
}
.topicList {
    width:880px;
    height:auto;
    border-top:1px solid #ECEBE9;
    padding-left:120px;
    position:relative;
    background:#F7F7F6;
}
.topicList .topicContent label {
    height:auto;
}
.topicList .addTopic_list {
    float: left;
}
.topicList .addtopic_box {
    float: left;
    width:100%;
    background:#FFF;
}
.topicList .addedTopic_box {
    float: left;
    width:100%;
    background:#FFF;
}
#content {
    float: left;
    display: inline;
    width:750px;
}
.addtopic_box .topicContent li label.la {
    float:left;
    width: 50px;
    font-size: 14px;
    color: #494949;
    font-weight: bold;
    background:none;
}
.addtopic_box .topicContent {
    float:left;
    width: 820px;
    margin-left:10px;
}
.addtopic_box .topicContent li.libox {
    float: left;
    margin-top: 20px;
    width: 100%;
}
.addtopic_box .topicContent li.btn {
    text-align:center;
}
.addtopic_box .topicContent li.libox .addTopicBtn,.cancel_addTopicBtn {
    width:174px;
    height:38px;
    line-height:38px;
    border:0;
    display: inline-block;
    margin:0 auto;
    cursor:pointer;
    color:#fff;
    font-size:14px;
    text-align:center;
}
.addtopic_box .topicContent li.libox .addTopicBtn{
    background:url('/apps/study/Public/Images/Home/btns.png') 0px 0px;
    margin-right:10px;
}
.addtopic_box .topicContent li.libox .addTopicBtn:hover {
    background:url('/apps/study/Public/Images/Home/btns.png') 0px -38px;
}
.addtopic_box .topicContent li.libox .cancel_addTopicBtn{
    background:url('/apps/study/Public/Images/Home/btns.png') -174px 0px;
}
.addtopic_box .topicContent li.libox .cancel_addTopicBtn:hover {
    background:url('/apps/study/Public/Images/Home/btns.png') -174px -38px;
}
.ad {
    float: left;

}
.ad a {
    color: #20D691;
    margin-right: 15px;
}
.dalist {
    float:left;
    width: 100%;
    margin-top: 10px;
    margin-left: 50px;
}
.dalist input {
    margin-right:3px;
}
.dalist label {
    margin-right:15px;
}
.dalist .pdt {
    display:inline;
    position: relative;
    top: -30px;
}
.dalist textarea {
    width:400px;
}
.dalist .jdt {
    display:inline;
    position: relative;
    top: -25px;
}
.spacelist label {
    margin-bottom:10px;
}
.spacelist input {
    margin-bottom:10px;
    width:300px;
    height:25px;
}
.addTopic_list li {
    float:left;
    width:830px;
    height:auto;
    padding-bottom:20px;
    margin-bottom:20px;
    border-bottom:1px solid #ECEBE9;
}
.addTopic_list li.noline {
    border-bottom:none;
}
.addTopic_list p.operate {
    padding:10px 30px;
    float: left;
    width: 770px;
    height: auto;
    margin-top:10px;
}
.addTopic_list p.operate label.lc {
    font-weight: normal;
    word-break: break-all;
    color: #20D691;
}
.addTopic_list p.operate label {
    margin-right: 16px;
    display: block;
    float: left;
}
.addTopic_list li .ttitle {
    width:100%;
    height:50px;
    line-height:50px;
    border-bottom: none;
    font-size: 14px;
    color: #20D691;
    margin-left:24px;
}
.addTopic_list li .tcontent {
    margin:20px 100px;
}
.addTopic_list li .tanswer {
    float:left;
}
.addTopic_list li .tanswer .dalist{
    margin-left:30px;
}
.create_homework .dalist1 label.option {
    float:none;
    width: 10px;
    background: none;
    text-align: center;
    font-size: 14px;
    color: #313030;
    font-weight: normal;
    height:10px;
}
.tanswer .dalist1 .pdt {
    position:relative;
    left:0;
}
.tanswer .dalist1 .jdt {
    position:relative;
    left:0;
}
.tanswer .dalist1 label {
    float:none;
    width: 10px;
    text-align: center;
    font-size: 14px;
    color: #313030;
    font-weight: normal;
    height:10px;
    margin-right:10px;
}
.addTopic_list .dalist1 li {
    margin-bottom:5px;
}
.addTopic_list .jdt textarea {
    width:600px;
}
.addTopic_list .dalist1 input {
    margin-right:3px;
}
.addTopic_list .dalist1 label {
    margin-right:15px;
}
.noline{
    border:none;
}
.dalist1 {
    margin-left:30px;
}
.submitBtn {
    margin:15px auto;
    text-align:center;
    overflow:hidden;
    width:368px;
}
.submitBtn button {
    margin:0px auto;
    text-align:center;
    border:none;
    font-size:14px;
    width:174px;
    height:38px;
    overflow:hidden;
    color:#fff;
}
.submitBtn .publish {
    margin-right:16px;
    background:url('/apps/study/Public/Images/Home/btns.png') 0px 0px;
}
.submitBtn .publish:hover {
    background:url('/apps/study/Public/Images/Home/btns.png') 0px -38px;
}
.submitBtn .finish {
    background:url('/apps/study/Public/Images/Home/btns.png') 0px 0px;
}
.submitBtn .finish:hover {
    background:url('/apps/study/Public/Images/Home/btns.png') 0px -38px;
}
/* 从题库中添加题目窗口 */
#reslibrary {
    height:570px;
    overflow:scroll;
    overflow-x:hidden;
}
#reslibrary .filter {

    float:left;
    width:650px;
    height:auto;
    margin-top:10px;
}
#reslibrary .filter li {
    float:left;
    width:100%;
    line-height: 45px;
    overflow: hidden;
}
#reslibrary .filter li label {
    width:80px;
    float:left;
    margin-left: 10px;
    font-size: 14px;
    color: #494949;
    font-weight: bold;
}
#reslibrary .filter li div {
    float: left;
    width: 550px;
}
#reslibrary .filter li span {
    float: left;
    height: 25px;
    line-height: 25px;
    padding: 0px 20px;
    border: 1px solid #20D691;
    margin: 10px 5px;
    cursor: pointer;
    position:relative;
}
#reslibrary .filter li span.on {
    background: #93E5BD;
}
#reslibrary .topic_search {
    margin-bottom:10px;
}
#reslibrary .topic_search input {
    float:left;
    width: 533px;
    height: 36px;
    line-height: 36px;
    margin-left: 10px;
    /*border: none;
    background: url('/Public/Images/Home/cc_input.png') no-repeat;*/
}
#reslibrary .topicOption {
    width:750px;
    height:auto;
    padding:10px;
    margin-bottom:10px;
}
#reslibrary .topicOption.on{
    background:#e0e0e0;
}
#reslibrary .topic_search button {
    float:left;
    width: 90px;
    height: 39px;
    border: none;
    margin-left: 10px;
    background: url('/apps/study/Public/Images/Home/searchBtn.png') no-repeat;
    cursor:pointer;
}
#reslibrary .sTopicList .topicOption span {
    float:left;
    width:60px;
    height:60px;
    line-height:60px;
    display:block;
    background:#E2F5E9;
    text-align:center;
    border-radius:2px;
}
#reslibrary .sTopicList .topicOption p {
    float:left;
    width:60px;
}
#reslibrary .sTopicList .topicOption p span {
    cursor:pointer;
    color:#797979;
    font-size:18px;
    font-weight:bold;
}
#reslibrary .sTopicList .topicOption p span.selected {
    background:#20D691;
}
#reslibrary .sTopicList .topicOption p a {
    float:left;
    width:60px;
    margin-top:6px;
    text-align:center;
}
#reslibrary .sTopicList .topicOption p a:hover{
    color:#20D691;
}
#reslibrary .sTopicList .topicOption .topicObj {
    width:650px;
    height:auto;
    padding:10px;
    margin-left:15px;
    border:1px solid #e0e0e0;
}
#reslibrary .topicObj div {
    float:left;
    width:100%;
    margin-bottom:6px;
    font-size:14px;
}
#reslibrary .topicObj .tcontent {
    float:left;
    width:100%;
}
#reslibrary .topicObj .tcontent p {
    float:left;
    width:100%;
    margin-bottom:5px;
}
#reslibrary .topicObj .ttitle {
    color:#20D691;
}
/* 学科领域标签添加窗口 */
#subject_area ul li {
    float:left;
    margin-top:10px;
}
#subject_area ul li label {
    float: left;
    width: 60px;
    margin-left: 10px;
    font-size: 14px;
    color: #494949;
    font-weight: bold;
    line-height:25px;
}
#subject_area ul li div {
    float: left;
    width: 660px;
}
#subject_area ul li div.addTag_list {
    width: 560px;
}
#subject_area ul li div.user_defined {
    float: left;
    width: 580px;
}
#subject_area ul li div span {
    display: inline-block;
    margin: 0 10px 3px 0;
    padding: 5px 20px;
    border: 1px solid #20D691;
    cursor: pointer;
    position:relative;
}
#subject_area ul li div span cite {
    position: absolute;
    right: -8px;
    top: -8px;
    width: 16px;
    height: 16px;
    background: url("/apps/study/Public/Images/Home/mould.png") no-repeat -367px -1px;
    cursor: pointer;
    display:none;
}
#subject_area li div span.on {
    background:#93E5BD;
}
#subject_area ul li label.list_lab {
    cursor:pointer;
}
#subject_area .tag_append {
    float: right;
    width: 78px;
    height: 38px;
    line-height: 37px;
    margin-left: 18px;
    text-align: center;
    background: url("/apps/study/Public/Images/Home/left_btn.png") no-repeat -0px -0px;
    color: #A5A5A5;
    cursor: pointer;
}
#subject_area .tag_append:hover {
    background: url("/apps/study/Public/Images/Home/left_btn_h.png") no-repeat -0px -0px;
    color: #109443;
}

/* 学生做作业 */
.do_homework {
    float:left;
    width:1000px;
    height:auto;
    background:#F3F2F0;
    margin:50px 0;
    position:relative;
    display: inline;
}
#book_ring {
    position:absolute;
    top:-18px;
    left:0;
    width:990px;
    height:44px;
    margin-left:10px;
    background:url('/apps/study/Public/Images/Home/ring.png') repeat-x;
}
.do_homework .title {
    float:left;
    width:100%;
    height:60px;
    line-height:60px;
    text-align:center;
    margin-top:20px;
    font-size:18px;
    font-weight:bold;
    color:#313030;
    position:relative;
    display:inline;
}
.do_homework .title cite {
    color:#949494;
    margin-left:10px;
}
.do_homework .title a {
    position:absolute;
    right:30px;
    font-weight:normal;
}
.do_homework .title a:hover {
    color:#22a558;
}
.do_homework .attachment {
    width:960px;
    padding:10px 20px;
    display:inline;
    /*border-top:1px solid #DDDAD5;*/
}
.do_homework .attachment .download {
    margin-bottom:10px;
}
.do_homework .attachment .download a {
    margin-right:20px;
    text-decoration: underline;
    color:#20D691;
}
.do_homework .attachment div label {
    font-size:14px;
    font-weight:bold;
}
.do_homework .dohw_list {
    width:960px;
}
.do_homework .dohw_list .topicObj {
    width:100%;
}
.do_homework .dohw_list .topicObj p.tpoic_top {
    float: left;
    width: 950px;
    padding: 10px 25px;
    border-top: 1px solid #DDDAD5;
    border-bottom: 1px solid #DDDAD5;
    font-size: 18px;
    color: #313030;
    font-weight:bold;
    display:inline;
}
.do_homework .dohw_list .topicObj p.answer_error {
    color:#F63D3D;
}
.dohw_list .topicObj .tpoic_main {
    width:100%;
    height:auto;
    float: left;
}
.dohw_list .topicObj .tpoic_main div.tpoic_main_r {
    float:left;
    margin-left:120px;
    display:inline;
    border-left:1px solid #DDDAD5;
    width: 880px;
}
.dohw_list .topicObj .tpoic_main  .content {
    float: left;
    width: 830px;
    padding: 25px;
    font-size: 14px;
    font-weight: bold;
    overflow:hidden;
    display:inline;
}
.dohw_list .topicObj .tpoic_main .answer {
    float: left;
    width: 830px;
    padding: 25px;
    font-size: 14px;
    color: #454645;
    color:#F63D3D;
    overflow:hidden;
    display:inline;
}
.dohw_list .topicObj .tpoic_main .s_answer {
    float: left;
    width: 830px;
    padding: 0 25px;
    font-size: 14px;
    color: #454645;
    padding-bottom:35px;
    overflow:hidden;
    display:inline;
}
.dohw_list .topicObj .tpoic_main .shorAnswer div {
    float:left;
    position:relative;
    margin-left: 65px;
    margin-bottom: 10px;
}
.dohw_list .topicObj .tpoic_main .shorAnswer div .del_shortAnswer{
    position:absolute;
    top:0px;
    right:0px;
}
.tpoic_main .s_answer label.la {
    float:left;
}
.tpoic_main .s_answer .student_answer {
    display:inline;
    float:left;
}
.tpoic_main .s_answer .student_answer span {
    margin-right:20px;
}
.tpoic_main .s_answer .student_answer span.right {
    color:#20D691;
}
.tpoic_main .s_answer .student_answer span.error {
    color:#F63D3D;
}
.tpoic_main .s_answer .student_answer input {
    margin-right:3px;
}
.tpoic_main .s_answer .student_answer label {
    margin-right:10px;
}
.dohw_bottom {
    position:fixed;
    bottom:0;
    background:#F3F2F0;
    width:100%;
    height:70px;
    line-height:23px;
    z-index:9999;
    border-top:1px solid #D9DACA;
    _bottom:auto;
    _width:100%;
    _position:absolute;
}
.dohw_bottom img {
    float:left;
    margin:12px 15px 0 55px;
}
.useSeconds{
    float:left;
    line-height:70px;
    font-size:18px;
    color:#e27e42;
}
.overTime{
    float:left;
    line-height:70px;
    margin-left:15px;
    font-size:14px;
    color:red;
}

.useSeconds cite {
    margin-right:10px;
}
.dohw_bottom .closeTimer,.resetTimer {
    float:left;
    line-height:70px;
    font-size:14px;
    color:#b85c5c;
    margin-left:25px;
    cursor:pointer;
}
.dohw_bottom .btn {
    margin-right:240px;
}
.dohw_bottom .btn a,.dohw_bottom .btn img {
    float:left;
    width:104px;
    height:38px;
    border:0;
    margin-top:15px;
    margin-left:20px;
    cursor:pointer;
}
.dohw_bottom .btn img {
    background:url('/apps/study/Public/Images/Home/tjhw.png');
}
.dohw_bottom .btn img.storage {
    background:url('/apps/study/Public/Images/Home/cg.png');
}
.dohw_bottom .btn a.savecg{
    background:url('/apps/study/Public/Images/Home/cg.png');
}
.dohw_bottom .btn a.close {
    background:url('/apps/study/Public/Images/Home/closehw.png');
}
/* 老师批作业 */
#left_sider div ul {
    margin: 10px 0 0 0;
}
.class_students ul li {
    float:left;
    width:60px;
    height:90px;
    text-align:center;
    margin-left:7px;
    margin-bottom:25px;
    display:inline;
}
.class_students ul li a {
    width:60px;
    overflow:hidden;
    white-space:nowrap;
    font-size:14px;
    color:#555;
}
.class_students ul li a.photo {
    cursor:pointer;
}
.class_students ul li a.photo img {
    border:2px solid #F3F2F0;
    filter:alpha(opacity=50);
    -moz-opacity:0.5;
    -khtml-opacity: 0.5;
    opacity: 0.5;
}
.class_students ul li a.photo img.on {
    border:2px solid #A5E8C6;
    filter:alpha(opacity=100);
    -moz-opacity:1;
    opacity: 1;
}
.class_students ul li span {
    color:#9d9d9d;
    cursor:pointer;
}
.class_students ul li span.tjed {
    color: #20D691;
    cursor:pointer;
}
.classhomework_top {
    float: left;
    width: 750px;
    height: 60px;
    overflow: hidden;
    margin-top: 25px;
    margin-left: 30px;
}
.classhomework_top li {
    float: left;
    width: 150px;
    height: 60px;
    line-height: 50px;
    margin-right: 20px;
    font-size: 18px;
    color: #99999D;
    text-align: center;
    cursor: pointer;
    background: url('/apps/study/Public/Images/Home/d_selected.png') no-repeat bottom;
}
.classhomework_top a.back {
    line-height:50px;
    font-size:18px;
    color:#080808;
    margin:0px 20px 0px 20px;
}
.classhomework_top a.choose_class {
    float:right;
    font-size:14px;
    color:#555;
    padding:10px 35px;
    margin-top: 8px;
    background:url('/apps/study/Public/Images/Home/chooseClass.png');
    cursor:pointer;
}
.classhw_box {
    float: left;
    width: 760px;
    height: auto;
    background: #F3F2F0;
    box-shadow: 2px 2px 2px rgba(0, 0, 0, 0.1);
    margin-left: 20px;
    margin-bottom: 30px;
    padding: 20px 0 30px 0;
    display: inline;
}
.classhw_box .hw_score {
    float:left;
    width:750px;
    height:115px;
    border-bottom:1px solid #DDDAD5;
    position:relative;
}
.classhw_box .hw_score div {
    float:left;
    width:740px;
    _width:700px;
    margin-top: 15px;
    margin-left:20px;
}
.do_homework div.precision {
    float:left;
    width:960px;
    margin-top: 10px;
    padding:20px;
