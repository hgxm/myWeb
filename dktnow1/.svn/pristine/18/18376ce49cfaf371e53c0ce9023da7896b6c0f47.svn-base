//加载课文列表UI
-(void)loadLeftScrollView:(NSMutableArray *)array{
    
    for (int i=0; i<[array count]; i++) {
        
        NSMutableDictionary *muDict = [array objectAtIndex:i];
        
        UIButton *btnLesson = [[UIButton alloc]initWithFrame:CGRectMake(i%3*(202+30), i/3*(52+15), 202, 52)];
        [btnLesson setBackgroundImage:[UIImage imageNamed:@"nkc课文按钮.png"] forState:UIControlStateNormal];
        
        NSString *str;
        str = [muDict objectForKey:@"l_title"];
        btnLesson.tag = i+3000;
        [btnLesson setTitleColor:TEXTCOLOR forState:UIControlStateNormal];
        [btnLesson addTarget:self action:@selector(btnLessonClicks:) forControlEvents:UIControlEventTouchDown];
        btnLesson.titleLabel.font = [UIFont boldSystemFontOfSize:17];
        btnLesson.titleLabel.numberOfLines = 0;
        [btnLesson setTitle:str forState:UIControlStateNormal];

        [btnLesson setContentHorizontalAlignment:UIControlContentHorizontalAlignmentCenter];
        [lessonScr addSubview:btnLesson];
        [btnLesson release];
    }
    [lessonScr setContentSize:CGSizeMake(680, (52+15)*[array count]/3+52)];
}
//点击课文，获取课时列表
-(void)btnLessonClicks:(UIButton *)btn{
    
    
    for (UIView *v in [hourScr subviews]) {
        [v removeFromSuperview];
    }
    
    if (indexSelect != 0) {
        
        UIButton *perBtn = (UIButton *)[lessonScr viewWithTag:indexSelect];
        [perBtn setBackgroundImage:[UIImage imageNamed:@"nkc课文按钮.png"] forState:UIControlStateNormal];
        [perBtn setTitleColor:TEXTCOLOR forState:UIControlStateNormal];

    }
    
    
    
    [btn setBackgroundImage:[UIImage imageNamed:@"nkc课文按钮1.png"] forState:UIControlStateNormal];
    [btn setTitleColor:[UIColor whiteColor] forState:UIControlStateNormal];

   
    
    indexSelect = btn.tag;
    
    NSDictionary *classhour=[LessonList objectAtIndex:btn.tag-3000];
    NSLog(@"classhour is %@",classhour);
    
    NSDictionary *dict=[[NSUserDefaults standardUserDefaults]objectForKey:@"loginInfo"];
    NSString *timeString =[ToolClass getCurretDate];
    NSMutableDictionary *postDict=[[NSMutableDictionary alloc]init ];
    [postDict setObject:timeString forKey:@"ts"];
    [postDict setObject:[dict objectForKey:@"auth_id"] forKey:@"args[a_id]"];
    [postDict setObject:[classhour objectForKey:@"l_id"] forKey:@"args[l_id]"];
    
    NSMutableDictionary *dictBanji = [kechenglist objectAtIndex:(perSelect-1000)];
    if ([[dictBanji objectForKey:@"c_id"] intValue] == 0) {
        
        [postDict setObject:[dictBanji objectForKey:@"cro_id"] forKey:@"args[cro_id]"];

    }else{
        [postDict setObject:[dictBanji objectForKey:@"c_id"] forKey:@"args[c_id]"];

    }

    [postDict setObject:@"JSON" forKey:@"format"];
    [postDict setObject:@"Classhour.lists" forKey:@"method"];
    [postDict setObject:[dict objectForKey:@"skey"] forKey:@"skey"];
    NSString *strSign=[ToolClass getSign:postDict];
    [postDict setObject:[MyMD5 md5:strSign] forKey:@"sign"];
    
    if (request) {
        [request cancel];
        request.delegate=nil;
        [request release];
        request=nil;
    }
    request=[[ASIFormDataRequest alloc]initWithURL:[NSURL URLWithString:[NSString stringWithFormat:@"%@/apps/manage/Api",dele.server_Address_str]]];
    [request setDelegate:self];
    for(int i=0;i<[[postDict allKeys] count];i++){
        [request setPostValue:[[postDict allValues]objectAtIndex:i] forKey:[[postDict allKeys]objectAtIndex:i]];
    }
    [request buildPostBody];
    [request setTimeOutSeconds:30];
    [request startAsynchronous];
    [request setDidFinishSelector:@selector(mySuccessForClassHour:)];
    [request setDidFailSelector:@selector(myError:)];
    [postDict release];
    [strSign release];
    
    showKeshiView.hidden = NO;
    
}
//加载课程成功
-(void)mySuccess:(id)sender{
    
    
    NSString *str=[sender responseString];
    
    NSMutableDictionary *mudict = [str JSONValue];
    
    NSLog(@"mudict is %@",mudict);
    kechenglist = [[[mudict objectForKey:@"info"] objectForKey:@"list"] mutableCopy];
    
    for (int i=0; i<[kechenglist count]; i++) {
        NSDictionary *dict=[kechenglist objectAtIndex:i];
        
        UIButton *btnCourse = [[UIButton alloc]initWithFrame:CGRectMake(i*(234), 0, 204, 152)];
        UIImage *btnImg = [UIImage imageNamed:@"nkc课程底正常.png"];
        btnImg = [btnImg stretchableImageWithLeftCapWidth:10 topCapHeight:10];
        [btnCourse setBackgroundImage:btnImg forState:UIControlStateNormal];
        btnCourse.tag=i+1000;
        [btnCourse addTarget:self action:@selector(selectBanji:) forControlEvents:UIControlEventTouchDown];
        [mainScrollView addSubview:btnCourse];
        [btnCourse release];
        
        
        ImgDownSelf *imageView = [[ImgDownSelf alloc]initWithFrame:CGRectMake(7, 5, 190, 106)];
        imageView.userInteractionEnabled = NO;
        imageView.image = [UIImage imageNamed:@"temp.png"];
        [imageView setImgUrl:[dict objectForKey:@"co_cover"]];
        [btnCourse addSubview:imageView];
        [imageView release];
        
        UILabel *lbl = [[UILabel alloc]initWithFrame:CGRectMake(10, 119, 190, 25)];
        lbl.text = [dict objectForKey:@"co_title"];
        lbl.textAlignment = NSTextAlignmentLeft;
        lbl.backgroundColor = [UIColor clearColor];
        lbl.tag = 1;
        lbl.textColor = TEXTCOLOR;
        lbl.userInteractionEnabled = NO;
        lbl.font = [UIFont boldSystemFontOfSize:17];
        [btnCourse addSubview:lbl];
        [lbl release];
    }
    
    UIButton *perBtn = (UIButton *)[mainScrollView viewWithTag:perSelect];
    [self selectBanji:perBtn];
    [mainScrollView setContentSize:CGSizeMake((234)*([kechenglist count]), 152)];
    
    
}


