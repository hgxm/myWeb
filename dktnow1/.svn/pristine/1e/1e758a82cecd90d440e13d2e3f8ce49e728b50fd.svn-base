/* 资源详情页 文档 */
body {
    background: url('/apps/resource/Public/Images/Home/vedio_body_bg.png') repeat;
}
.doc_box {
    float:left;
    width:100%;
    height:auto;
    position:relative;
}
.doc_box .top_dir {
    width:100%;
    height:50px;
    line-height:50px;
    font-size:14px;
    color:#2DB35D;
    font-family: arial,helvetica,clean;
}
.doc_box .main_box {
    float:left;
    width:1000px;
    height:auto;
}
.doc_box .main_box .main-left {
    float: left;
    width: 760px;
    height: auto;
    background:#F9FDFC;
    border: 1px solid #E4F7EB;
    border-left: none;
}
.doc_box .main_box .main-left_top {
    float:left;
    width:760px;
    height:auto;
    background:#F9FDFC;
    /*border:1px solid #E4F7EB;
    box-shadow: 0 0 2px 2px rgba(0, 0, 0, 0.05);*/
}
.doc_box .main_box .main-left_top .f_tit {
    float:left;
    margin:10px 0px 20px 20px;
    color:#666;
    display:inline;
}
.doc_box .main_box .main-left_top .doc_name {
    float:left;
    width:720px;
    margin:15px 20px 0px 20px;
    display:inline;
}
.doc_box .main_box .main-left_top .doc_name img {
    float:left;
    margin-top:5px;
    margin-right:5px;
}
.doc_box .main_box .main-left_top .doc_name .tt {
    font-size: 20px;
    font-family: '微软雅黑','黑体',arial;
    font-weight: bold;
    color: #333;
    cursor:pointer;
}
.doc_box .main_box .main-left_top .doc_name .rr {
    font-size: 14px;
    font-weight: normal;
    color: #666;
    line-height:27px;
    margin-top:3px;
    margin-left:5px;
}
.doc_box .main_box .main-left_top .doc_name .jb {
    background:url('/apps/resource/Public/Images/Home/jb.png') no-repeat;
    padding-left: 18px;
    color:#333;
    margin-top:6px;
    cursor:pointer;
}
#documentViewer {
    float:left;
    width:760px;
    height:560px;
}
.main-left_top .tools {
    float:left;
    width:720px;
    padding:20px;
}
.main-left_top .tools .sc {
    float:left;
    width:156px;
    height:38px;
    background:url('/apps/resource/Public/Images/Home/doc_tools.png') no-repeat 0px -77px;
    cursor:pointer;
    margin-right:25px;
}
.main-left_top .tools .dw {
    float:left;
    width:99px;
    height:38px;
    background:url('/apps/resource/Public/Images/Home/doc_tools.png') no-repeat 0px -39px;
}
.main-left_top .tools .dw_info {
    float:left;
    width:99px;
    height:36px;
    line-height:36px;
    text-align:center;
    border:1px solid #E2F3EC;
    background:#EBFAF4;
    color:#9A9691;
}
.like-tit {
    float:left;
    width:745px;
    padding-left: 15px;
    padding-bottom: 6px;
    font-size: 14px;
    color: #333;
    font-weight: bold;
}
.main_left_bo {
    float: left;
    width: 760px;
    height: auto;
    margin-top:20px;
}
.main_left_bo .ylike_box {
    float: left;
    width: 760px;
    height: auto;
    background: #fff;
    /*border: 1px solid #E4F7EB;
    box-shadow: 0 0 2px 2px rgba(0, 0, 0, 0.05);*/
}
.main_left_bo .ylike_box_top {
    float: left;
    background: #F9FDFC;
    width:100%;
    height:auto;
}
.main_left_bo .ylike_box_top ul {
    float:left;
    padding:18px 0 3px 10px;
    border-bottom:1px solid #ECF8F4;
}
.main_left_bo .ylike_box_top ul li{
    float:left;
    padding:3px 5px;
    border:1px solid #ACD2C5;
    background:#D3EAE4;
    margin-right:10px;
    margin-bottom:15px;
    overflow:hidden;
    text-overflow: ellipsis;
    white-space:nowrap;
    cursor:pointer;
}
.main_left_bo .like_list li {
    float:left;
    width:740px;
    height:30px;
    line-height:30px;
    padding:2px 0 2px 20px;
}
.main_left_bo .like_list li img {
    float:left;
    margin-top:5px;
    margin-right:5px;
}
.main_left_bo .like_list li span {
    float:left;
    height:30px;
    line-height:30px;
}
.main_left_bo .like_list li span.d-name {
    width:400px;
    margin-right:20px;
}
.main-right .doc_tj ul.d-start {
    width:90px;
    padding-bottom:0;
}
.main_left_bo .like_list ul.d-start li ,.main-right .doc_tj ul.d-start li {
    float: left;
    width: 12px;
    height: 12px;
    margin:8px 5px 0 0;;
    background: url('/apps/resource/Public/Images/Home/v-star.png') 0px 0px;
    padding:0;
}
.main_left_bo .like_list li .d-page {
    width:60px;
    color:#999;
    margin-left:10px;
}
.main_left_bo .like_list li .d-p {
    color:#999;
    margin-left:50px;
}
.main_left_bo .like_list li.more {
    cursor:pointer;
}
.main-right {
    float:left;
    width:235px;
    height:auto;
    border:1px solid #FAF9F8;
    background:#fff;
    display:inline;
    overflow:hidden;
}
.main-right .upload_area {
    float:left;
    display:inline;
    width:203px;
    height:37px;
    background: url('/apps/resource/Public/Images/Home/doc_tools.png') 0px -2px;
    margin-top:25px;
    margin-left:15px;
    cursor:pointer;
}
.main-right .upload_area:hover {
    background: url('/apps/resource/Public/Images/Home/doc_tools.png') 0px -115px;
}
.main-right .doc_info {
    float:left;
    margin-top:25px;
    width: 100%;
}
.main-right .doc_info .tit {
    margin-left:15px;
    color:#424242;
    font-weight:bold;
    font-size:14px;
}
.main-right .doc_info .author {
    margin-top:10px;
    margin-left:15px;
    font-size:12px;
    color:#999;
}
.main-right .doc_info .author cite {
    color:#2DB35D;
    margin-right:5px;
}
.main-right .doc_info .give_score .show {
    float:left;
    width:120px;
    height:auto;
    margin-top:10px;
    margin-left:15px;
    display:inline;
}
.main-right .doc_info .give_score .show .get_score {
    float:left;
    font-size:24px;
    color:#FF8300;
    font-weight:bold;
}
.main-right .doc_info .give_score .show .star {
    float:left;
}
.main-right .doc_info .give_score .show .star i {
    float: left;
    width: 12px;
    height: 12px;
    margin-top: 8px;
    margin-right:2px;
    background: url('/apps/resource/Public/Images/Home/v-star.png') 0px 0px;
}
.main-right .doc_info .give_score .show .bot {
    color:#999;
}
.main-right .doc_info .give_score .give {
    float:left;
    margin-top:15px;
}
.main-right .doc_info .give_score .give a {
    width: 89px;
    height: 22px;
    display: inline-block;
    background:url('/apps/resource/Public/Images/Home/doc-page.png') no-repeat 0px -133px;
    padding: 8px 0;
    text-align: center;
    text-decoration: none;
    outline: 0;
    color: #333;
    font-size: 14px;
}
.main-right .doc_info .give_score .give a .ic-value {
    display: inline-block;
    padding: 0;
    font-size: 0;
    line-height: 9999em;
    overflow: hidden;
    position: relative;
    width: 19px;
    height: 18px;
    vertical-align: -4px;
    background:url('/apps/resource/Public/Images/Home/doc-page.png') no-repeat -75px -176px;
}
.main-right .doc_tj {
    float:left;
    width:100%;
}
.main-right .doc_tj p {
    float:left;
    width:205px;
    height:20px;
    line-height:20px;
    margin:30px 15px 10px 15px;
    color:#2DB35D;
    font-weight:bold;
    font-size:14px;
    border-bottom:1px solid #F6F6F5;
    display:inline;
}
.main-right .doc_tj ul {
    float:left;
    width:100%;
    padding-bottom:8px;
}
.main-right .doc_tj ul li {
    float:left;
    width:205px;
    margin:0px 15px 10px 15px;
}
.main-right .doc_tj ul li img {
    float:left;
    margin-right:8px;
}
.main-right .doc_tj ul li .d-name {
    float:left;
    width:180px;
    overflow:hidden;
    white-space:nowrap;
    text-overflow: ellipsis;
    cursor:pointer;
}
.main-right .doc_tj ul li .d-start {
    margin-left:22px;
}
.main-right .doc_tj ul li .d-start i {
    float: left;
    width: 12px;
    height: 12px;
    margin-top: 5px;
    margin-right: 3px;
    background: url('/apps/resource/Public/Images/Home/v-star.png') 0px 0px;
}
.main-right .doc_tj ul li .d-page {
    margin-top:3px;
    margin-left:5px;
    width:60px;
    color:#999;
    overflow:hidden;
    white-space:nowrap;
}
.main-right .doc_tj ul li .d-p {
    margin-top:3px;
    color:#999;
}
/* 我要评价 弹出窗口 */
#comment .my_opinion {
    float:left;
    width:100%;
    margin-top:20px;
}
#comment .my_opinion ul {
    float: left;
    display: inline;
    height: 19px;
    line-height: 19px;
}
#comment .my_opinion label {
    float:left;
    font-size: 14px;
    color: #333;
    vertical-align: middle;
    margin-right:10px;
}
#comment .my_opinion ul li {
    float: left;
    width: 24px;
    cursor: pointer;
    text-indent: -9999px;
    background: url('/apps/resource/Public/Images/Home/star.png') no-repeat;
}
#comment .my_opinion ul li.on {
    background: url('/apps/resource/Public/Images/Home/star.png') no-repeat 0 -28px;
}
#comment .my_opinion .img-tip {
    background: no-repeat;
    display: inline-block;
    position: absolute;
    width: 240px;
    height: 40px;
    top: 15px;
    background:url('/apps/resource/Public/Images/Home/doc-page.png') no-repeat 0px 0px;
}
#comment .impress_tag {
    float:left;
    width:100%;
    margin-top:40px;
}
#comment .impress_tag p cite {
    color: #E87301;
    margin-left:10px;
}
#comment .impress_tag ul {
    float:left;
    width:100%;
    margin-top:10px;
}
#comment .impress_tag ul li {
    float:left;
    padding:3px 15px;
    margin-top:10px;
    margin-right:6px;
    cursor:pointer;
}
#comment .impress_tag ul li.good ,#comment .impress_tag ul li.defult{
    border: 1px solid #F0C9A4;
    background: #FFF3E5;
    color: #E87301;
}
#comment .impress_tag ul li.poor {
    border: 1px solid #D1D1D1;
    background: #EAE9E9;
    color: #666;
}
#comment .impress_tag ul li.tag-choose {
    background: url("/apps/resource/Public/Images/Home/hitchoose.png") no-repeat;
    background-position: right top;
    cursor: pointer;
}
#comment .impress_tag .input-box {
    border: 1px solid #D4D4D4;
    text-align: left;
    display: inline-block;
    width: 109px;
    padding: 0;
    margin-top:10px;
}
#comment .impress_tag .input-box input {
    outline: 0;
    width: 79px;
    _height: 11px;
    border: 0;
    color: #666;
    display: inline-block;
    padding: 3px 0 3px 5px;
    background-color: transparent;
}
#comment .impress_tag .input-box .add-btn {
    background: no-repeat;
    display: inline-block;
    position: absolute;
    left: 99px;
    width: 23px;
    height: 22px;
    border-left: 1px solid #D4D4D4;
    outline: 0;
    background:url('/apps/resource/Public/Images/Home/doc-page.png') no-repeat;
    background-position: -188px -133px;
}
/* 评论区域 */
.main-left-center {
    float:left;
    width:760px;
    margin-top: 20px;
}
.main-left-center .res_comment {
    float: left;
    width: 760px;
    height: auto;
    margin-top: 15px;
    box-shadow: 0 0 2px 2px rgba(0, 0, 0, 0.05);
}
.main-left-center .res_comment .title {
    width: 740px;
    height: 45px;
    line-height: 45px;
    color: #666;
    font-size: 15px;
    font-weight: bold;
    margin-left: 20px;
}
.main-left-center .res_comment .topic_input {
    float: left;
}
.main-left-center .res_comment .topic_input textarea {
    display: block;
    width: 696px;
    height: 80px;
    background-color: white;
    padding: 5px 10px;
    font-size: 14px;
    line-height: 20px;
    overflow: auto;
    border: 1px solid #D5D5D5;
    margin-bottom: 10px;
    margin-left: 20px;
    box-shadow: inset 1px 1px 2px #DFDFDF;
}
.main-left-center .res_comment .topic_input .sendbox {
    width:738px;
    height: 24px;
    line-height: 24px;
}
.topic_input button {
    width: 66px;
    height: 24px;
    text-align: center;
    color: white;
    font-size: 14px;
    background: url('/apps/resource/Public/Images/Home/sprite.png');
    border: none;
    float: right;
    margin-left: 5px;
    cursor: pointer;
}
.scommentInputCount, .innerCount {
    color: #969696;
    float: right;
    margin-left: 20px;
    line-height: 24px;
    font-size: 14px;
}
.res_comment ul.tabbox {
    float: left;
    margin-top: 20px;
    margin-left: 15px;
    display:inline;
}
.res_comment ul.tabbox li {
    float: left;
    display: inline-block;
    width: 61px;
    height: 20px;
    background: white;
    margin-left: 5px;
    color: #333;
    line-height: 20px;
    text-align: center;
    border: solid #E7E7E7 1px;
    cursor: pointer;
    overflow: hidden;
}
.res_comment ul.tabbox li.on {
    width: 64px;
    height: 25px;
    background: url('/apps/resource/Public/Images/Home/sprite_tt.png');
    background-position: -46px 0px;
    color: white;
    line-height: 22px;
    border: none;
    display:inline;
}
.res_comment .comment_box {
    width: 740px;
    height: auto;
    margin-left: 20px;
    display:inline;
}
.res_comment .comment_box ul {
    margin-top: 5px;
}
.res_comment .comment_box ul li {
    float: left;
    width: 720px;
    padding: 13px 0;
    line-height: 24px;
    border-bottom: dotted #CCC 1px;
}
.res_comment .comment_box ul li.hf {
    float: left;
    width: 700px;
    padding: 13px 0;
    line-height: 24px;
    border-bottom: dotted #CCC 1px;
}
.res_comment .comment_box ul li .des {
    color: #333;
    word-break: break-all;
    word-wrap: break-word;
}
.res_comment .comment_box ul li .fun {
    position: relative;
    height: 24px;
}
.res_comment .comment_box ul li .fun .from {
    float: left;
    color: #999;
}
.res_comment .comment_box ul li .fun .from a {
    color: #00AC4F;
    margin-right: 10px;
}
.res_comment .comment_box ul li .fun .ope {
    color: #CCC;
    float: right;
}
.res_comment .comment_box ul li .fun .ope a {
    color: #999;
    margin: 0px 5px;
}
.replyList {
    display: block;
    margin-left: 20px;
}
.innerText {
    float: left;
    display: block;
    width: 670px;
    height: 80px;
    background-color: white;
    padding: 5px 10px;
    font-size: 14px;
    line-height: 20px;
    overflow: auto;
    border: 1px solid #D5D5D5;
    margin-bottom: 10px;
    margin-left: 20px;
    margin-top: 10px;
    box-shadow: inset 1px 1px 2px #DFDFDF;
}
.innerButton {
    width: 66px;
    height: 24px;
    text-align: center;
    color: white;
    font-size: 14px;
    background: url('/apps/resource/Public/Images/Home/sprite.png');
    border: none;
    float: right;
    margin-left: 5px;
    cursor: pointer;
    margin-right: 10px;
}
.scommentInputCount, .innerCount {
    color: #969696;
    float: right;
    margin-left: 20px;
    line-height: 24px;
    font-size: 14px;
}
.scommentInputCount.overWord, .innerCount.overWord{
    color:red;
}
.innerDiv {
    display:none;
}
.res_comment .comment_box .moreArea {
    float: right;
    padding: 15px 25px 15px 0;
    text-align: right;
}
.res_comment .comment_box .moreArea a {
    color: #797979;
    margin-left: 10px;
}
.res_comment .comment_box .moreArea a:hover {
    color:#00AC4F;
    text-decoration:underline;
}