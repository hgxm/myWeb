    float:left;
    width:310px;
    height:38px;
    margin-top:15px;
    margin-left:15px;
    text-indent:10px;
    font-size:14px;
}
.main_left .search_box .common_search a {
    float: left;
    width:86px;
    height:39px;
    border:none;
    margin-top:15px;
    margin-left:15px;
    cursor:pointer;
}
.main_left .search_box .common_search .ks {
    background:url('/apps/resource/Public/Images/Home/resSearchBtn.png') no-repeat 0px 0px;
}
.main_left .search_box .common_search .ks:hover {
    background:url('/apps/resource/Public/Images/Home/resSearchBtn.png') no-repeat -86px 0px;
}
.main_left .search_box .common_search .gj {
    background:url('/apps/resource/Public/Images/Home/resSearchBtn.png') no-repeat 0px -39px;
}
.main_left .search_box .common_search .gj:hover {
    background:url('/apps/resource/Public/Images/Home/resSearchBtn.png') no-repeat -86px -39px;
}

.main_left .search_box .high_search {
    float:left;
    width:755px;
    height:auto;
}
.main_left .high_search .filter {
    width:683px;
    margin:16px 36px;
}
.main_left .high_search .filter input {
    margin-right:20px;
}
.main_left .high_search .filter span {
    font-size:14px;
    color:#606060;
    margin-right:45px;
}
.main_left .high_search ul {
    float:left;
    border-bottom:1px solid #DFDCD8;
    padding-bottom:15px;
}
.main_left .high_search ul li {
    float:left;
    width:683px;
    height:auto;
    margin:12px 36px 0 36px;
}
.main_left .high_search ul li label {
    float:left;
    width:83px;
    line-height:28px;
    font-size:14px;
    color:#292929;
}
.main_left .high_search ul li div {
    float:left;
    width:600px;
}
.main_left .high_search ul li span {
    float:left;
    padding:3px 20px;
    font-size:14px;
    color:#606060;
    border:1px solid #00C165;
    margin-right:12px;
    cursor:pointer;
}
.main_left .high_search ul li span.on {
    background:#93E5BD;
}
.main_left .high_search .btn {
    float: left;
    height: 55px;
    margin-top:15px;
    margin-left:200px;
    display:inline;
}
.main_left .high_search .btn a {
    float:left;
    width:174px;
    height:38px;
    line-height:38px;
    cursor:pointer;
    font-size:14px;
    text-align:center;
}
.main_left .high_search .btn .confirm {
    color:#fff;
    background:url('/apps/resource/Public/Images/Home/btns.png') 0px 0px;
    margin-right:30px;
}
.main_left .high_search .btn .confirm:hover {
    background:url('/apps/resource/Public/Images/Home/btns.png') 0px -38px;
}
.main_left .high_search .btn .cancel {
    color:#292929;
    background:url('/apps/resource/Public/Images/Home/btns.png') -174px 0px;
}
.main_left .high_search .btn .cancel:hover {
    background:url('/apps/resource/Public/Images/Home/btns.png') -174px 38px;
}
.main_left .res_list .res_tab {
    float: left;
    width: 755px;
    height: auto;
    overflow: hidden;
    position:relative;
}
.main_left .res_list .res_tab li {
    float: left;
    width: 150px;
    height: 50px;
    line-height: 50px;
    margin-right: 20px;
    font-size: 18px;
    color: #99999D;
    text-align: center;
    cursor: pointer;
}
.main_left .res_list .res_tab li.current {
    color: #333;
    background: url('/apps/resource/Public/Images/Home/d_selected.png') no-repeat bottom;
    
}
.main_left .res_list .res_box {
    width:755px;
    height:auto;
    background:#F3F2F0;
}
.main_left .res_list .res_box .mould {
    float:left;
}
.main_left .res_list .res_box .mould .more {
    margin-right: 25px;
    margin-top: 10px;
    font-size: 14px;
    color:#797979;
    font-weight: normal;
    cursor:pointer;
    background:url('/apps/resource/Public/Images/Home/more.png') no-repeat center;
    text-indent:-28px;
}
.main_left .res_list .res_box ul {
    float:left;
    width:750px;
    padding-left: 5px;
    padding-bottom:10px;
    margin-top:10px;
    border-bottom:1px solid #DFDCD8;
    display:inline;
    _margin:0;
}
.main_left .res_list .res_box ul li {
    float:left;
    width:121px;
    height:auto;
    margin-bottom:30px;
}
.main_left .res_list .res_box li a ,.main_left .res_list .res_box li span{
    float:left;
    margin-left:15px;
    display:inline;
}
.main_left .res_list .res_box .res_cover img {
    float:left;
    width:100px;
    height:75px;
    border:3px solid #fff;
    border-radius:4px;
}
.main_left .res_list .res_box li a.res_title {
    float:left;
    width:104px;
    font-size:14px;
    color:#080808;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
    margin-top:5px;
}
.main_left .res_list .res_box li span {
    font-size:14px;
    color:#A09C96;
}
.main_left .res_list .res_box li span i {
    float: left;
    max-width: 30px;
    overflow: hidden;
    color:#F44D4B;
}
.main_left .res_list .res_box .column_title {
    float:left;
    width:714px;
    font-size:18px;
    font-weight:bold;
    color:#080808;
    margin-top:10px;
    margin-bottom:16px;
    margin-left:36px;
    display:inline;
}
.main_left .res_list .res_box .column2 .alert {
    float:left;
    width:100%;
    font-size:18px;
    color:#99999d;
    text-align:center;
    margin:20px 0;
}
.main_right {
    float:left;
    width:235px;
    height:auto;
    margin:25px 0 20px 0;
}
.main_right div p.title {
    width:195px;
    font-size:18px;
    font-weight:bold;
    color:#363636;
    padding:15px 20px;
    border-bottom:1px solid #E0DDD9;
    background:#F3F2F0;
    border-radius: 3px 3px 0px 0px;
}
.main_right .file .file_tab ,.main_right .proffer .proffer_tab {
    float:left;
    width:235px;
    background:#F3F2F0;
}
.main_right .file .file_tab li {
    float:left;
    width:78px;
    height:45px;
    line-height:45px;
    text-align:center;
    font-size:14px;
    color:#969696;
    position:relative;
}
.main_right .file .file_tab li img {
    position:absolute;
    top:51px;
    left:32px;
}
.main_right .file .file_tab li span {
    cursor:pointer;
}
.main_right .file .file_tab li.current {
    color:#363636;
    font-weight:bold;
}
.main_right .file .file_box {
    float:left;
    position:relative;
    margin-top:15px;
}
.main_right .file .file_box div {
    float:left;
    width:235px;
    height:345px;
    background:#F3F2F0;
}
.main_right .file .file_box div.selected {
    background: url('/apps/resource/Public/Images/Home/d_selected.png') no-repeat top;
}
.main_right .file .file_box ul {
    margin-top:18px;
}
.main_right .file .file_box ul li {
    float:left;
    width:225px;
    height:22px;
    line-height:22px;
    margin-left:10px;
    margin-bottom:10px;
    display:inline;
}
.main_right .file .file_box ul li span.good ,.main_right .file .file_box ul li span.normal{
    float: left;
    width: 22px;
    height: 22px;
    line-height: 22px;
    text-align: center;
    margin-right: 10px;
}
.main_right .file .file_box ul li span.good {
    color: #fff;
    background: url('/apps/resource/Public/Images/Home/serialNum_good.png') no-repeat;
}
.main_right .file .file_box ul li span.normal {
    color: #3b3b3b;
    background: url('/apps/resource/Public/Images/Home/serialNum.png') no-repeat;
}
.main_right .file .file_box ul li label {
    float:left;
    width:180px;
    font-size:13px;
    color:#3b3b3b;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
}
.main_right .zt_res {
    margin-top:25px;
}
.main_right .zt_res .ztres_list {
    float:left;
    width:235px;
    height:290px;
    background:#F3F2F0;
}
#scroLeft {
    float: left;
    width: 227px;
    height: 100%;
    overflow: hidden;
}
#scroRight {
    background: #999;
    background: rgba(0, 0, 0, 0.1);
    float: right;
    height: 100%;
    width: 8px;
    border-radius: 10px;
    overflow: hidden;
    position: relative;
}
#scroLine {
    position: absolute;
    z-index: 1;
    top: 0;
    right: 0;
    width: 8px;
    background: url('/apps/resource/Public/Images/Home/opacity_70.png');
    background: rgba(0, 0, 0, 0.3);
    opacity: 0.7;
    border-radius: 10px;
    cursor: pointer;
}
#scroLeft li {
    float:left;
    width:227px;
}
#scroLeft li a.photo {
    float:left;
    margin-top:10px;
    margin-left:10px;
}
#scroLeft div {
    float:left;
    width:110px;
    margin-top:10px;
    margin-left:10px;
}
#scroLeft div a {
    width:110px;
    font-size:14px;
    color: #080808;
}
#scroLeft div p {
    width:110px;
    height:35px;
    font-size:12px;
    color:#A5A19B;
    overflow:hidden;
}
.main_right .proffer {
    margin-top:25px;
}
.main_right .proffer .proffer_tab li {
    float:left;
    width:117px;
