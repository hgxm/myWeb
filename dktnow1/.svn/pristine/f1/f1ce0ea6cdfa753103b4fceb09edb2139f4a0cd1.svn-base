    margin-top:5px;
    text-indent:50px;
    background:url('../Images/lesson.png') no-repeat -120px -38px;
    cursor:pointer;
}
.lesson_add:hover {
    background-position:-120px 0px;
}
.lesson_content {
    float:left;
    width:766px;
}
.lesson_caption {
    width:746px;
    height:60px;
    padding-left:20px;
    background:#26a75b;
    filter:alpha(opacity=70);
    opacity:0.7;
}
div.gray {
    background:#7C7B7B;
}
.lesson_caption li {
    float:left;
    height:60px;
    line-height:60px;
    text-align:center;
    color:#ffffff;
    font-size:14px;
}
.lesson_caption li.cap_order {
    width:10%;
}
.lesson_caption li.cap_name,.lesson_caption li.pub_name {
    width:30%;
    display:block;
    overflow:hidden;
}
.lesson_caption li.cap_name:hover,.pub_name:hover ,.pub_in:hover{
    cursor:pointer;
}
/*.lesson_caption li.cap_tag {
    width:15%;
    cursor:pointer;
}*/
.lesson_caption li.cap_time {
    width:30%;
}
.cap_down,.cap_up {
    width:3%;
}
.cap_operate {
    width:30%;
}
.cap_less {
    width:7%;
    cursor:pointer;
}
.cap_issue {
    width:10%;
    /width:9%;
    _width:9%;
}
.lesson_sub li.cap_close {
    width:12px;
    height:12px;
    margin-left:5px;
}
.lesson_sub li.cap_close img {
    display:none;
    cursor:pointer;
}

.lesson_list .lesson_sub li.cap_close img {
    display:none;
    cursor:pointer;
}
.lesson_list:hover .lesson_sub li.cap_close img {
    display:inline;
}
.pub_order {
    width:10%;
}
.pub_name {
    width:30%;
}
.pub_obj {
    width:20%;
}
.pub_time {
    width:20%;
}
.pub_operate {
    width:20%;
}
.pub_in {
    width:20%;
}
.cap_tag p {
    position:relative;
}
.cap_tag p span {
    position:absolute;
