-(void)myError:(id)sender{
    
    UIAlertView *alert = [[UIAlertView alloc] initWithTitle:@"提示" message:@"请求失败，请检查您的网络链接!" delegate:nil cancelButtonTitle:@"确定" otherButtonTitles:nil, nil];
    [alert show];
