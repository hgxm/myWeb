.mainBox {
    width: 765px;
    padding-top: 38px;
    margin-left: 20px;
    _margin-left: 19px;
    margin-bottom: 20px;
}
.mainBox .proof_title,.mainBox .proof_dir ,.mainBox .proof_back{
    font-size:24px;
    margin: 20px 0 30px 10px;
}
.proof_title {
    color:#3aba6e;
}
.choose_school,.choose_class{
    width:766px;
    height:auto;
    background: #F3F2F0;
    box-shadow: 2px 2px 2px rgba(0, 0, 0, 0.1);
    display: inline;
    border-radius:3px;
    margin-top:20px;
    position:relative;
}
.choose_school{
    margin-top:30px;
    padding-bottom:24px;
    z-index:10;
}
.choose_school h1, .choose_class h1{
    color:#080808;
    font-size:18px;
    height:54px;
    line-height:54px;
    padding-left:26px;
    border-bottom:1px solid #E4E2DF;
}
.choose_school div.in, .choose_school div.out{
    float:left;
    padding-left:26px;
    margin-top:20px;
    position:relative;
}
.choose_school div label{
    float:left;
    color:#676767;
    font-size:18px;
}
.choose_school .school_list{
    float:left;
    width:600px;
}
.choose_school .school_list *{
    font-size:18px;
    color: #676767;
}
.choose_school .school_list span{
    margin-left:15px;
    margin-right:30px;
}
.choose_school .school_list span input{
    margin-right:5px;
}
.choose_school .out input{
    width: 363px;
    height: 39px;
    line-height: 39px;
    border: none;
    background: url('/apps/study/Public/Images/Home/cc_input.png') no-repeat;
    margin-left:15px;
}
.choose_school .out label{
    margin-top:5px;
}
.choose_class{
    width:100%;
    height:auto;
    position:relative;
    margin-top:35px;
}
.choose_list{
    width:515px;
    height:auto;
    background:#FFF;
    position:absolute;
    top:158px;
    left:130px;
    z-index:100;
    display:none;
    border:1px solid #E6E4E1;
    padding:0 14px;
}
.choose_list p{
    float:left;
    width:100%;
    height:45px;
    line-height:45px;
    font-size:14px;
    color:#8F8C86;
}
.choose_list p span cite{
    font-size:16px;
    color:#E97A75;
    font-weight:bold;
}
.close_choose_list{
    float:right;
    width:10px;
    height:10px;
    background:url('/apps/study/Public/Images/Home/close_choose_list.png') no-repeat;
    margin-top:18px;
    cursor:pointer;
    display:inline;
}
.school_address label.addr{
    font-size:16px;
    color:#080808;
    font-weight:bold;
}
.school_address select{
    margin-left:10px;
}
.search_result{
    width:100%;
    height:auto;
    min-height:5px;
     /* sets min-height for IE */
     _height:expression(this.scrollHeight < 5 ? "5px" : "auto");
    max-height:120px;
    /* sets max-height for IE */
    _height:expression(this.scrollHeight > 120 ? "120px" : "auto");
    overflow-y:scroll;
    border-top:1px dotted #DCD9D4;
    border-bottom:1px dotted #DCD9D4;
    margin:10px 0;
}
.mainBox a.addlink{
    font-size:12px;
    color:#080808;
    margin:0 18px 10px 0;
}
.mainBox a.addlink cite{
    color:#E97A75;
    cursor:pointer;
}
.search_result ul{
    float:left;
    padding:10px 0;
}
.search_result ul li{
    float:left;
    width:160px;
    margin:3px 0;
    white-space:nowrap;
    text-overflow:ellipsis; 
    overflow: hidden;
    cursor:pointer;
}
.search_result ul li:hover{
    color:#3ABA6E;
}
.choose_class ul{
    width:100%;
    float:left;
}
.choose_class ul.class_tab li{
    float:left;
    width:90px;
    height:50px;
    line-height:50px;
    overflow:hidden;
    color:#080808;
    font-size:18px;
    text-align:center;
    cursor:pointer;
}
.choose_class ul li.current{
    color:#99999d;
    background: url('/apps/study/Public/Images/Home/currentBg.png') no-repeat bottom;
}
.applyClass{
    float:right;
    margin-right:23px;
    line-height:50px;
}
.applyClass a{
    color:#E97A75;
    cursor:pointer;
}
.class_list_box div{
    float:left;
    width:100%;
    height:auto;
    border-top:1px solid #E4E2DF;
}
.class_list_box div ul{
    width:720px;
    float:left;
    margin:20px 22px;
    display:inline;
}
.class_list_box div ul li{
    float:left;
    width:100%;
    margin-bottom:30px;
}
.class_list_box div ul li div.box_left{
    width:235px;
    height:116px;
    background: #F3F2F0;
    _border:1px solid #e3e3e3;
    box-shadow: 0  0 2px 2px rgba(0, 0, 0, 0.1);
    overflow: hidden;
}
.class_list_box div ul li a{
    float:left;
}
.class_list_box div ul li a img{
    float:left;
    margin:10px;
}
.class_list_box .class_name *{
    float:left;
    width:110px;
    overflow:hidden;
    font-size:16px;
    line-height:25px;
    margin-top:10px;
}
.class_list_box .class_name a{
    color:#2c2c2c;
}
.class_list_box .class_name span{
    color:#7e7e7e;
}
.noclass{
    width:100%;
    text-align:center;
    color:#888888;
    font-size:18px;
    margin-top:10px;
}
.box_right a{
    color:#c95858;
    margin-top:15px;
}
.insert_list{
    float:left;
    padding-bottom:10px;
}
.box_right{
    width:60px;
}

/* 添加学校 */
.add_school{
    width:1000px;
    height:auto;
    background:#fff;
    margin-top:25px;
}
.add_school .tit{
    width:100%;
    height:90px;
    line-height:90px;
    border-bottom:1px solid #ECEBE9;
}
.add_school .tit span{
    float:left;
    font-size:24px;
    color:#313030;
    margin-left:20px;
}
.add_school .tit a{
    float:right;
    font-size:24px;
    color:#313030;
    margin-right:45px;
}
.add_school .tit a:hover{
    color:#22a558;
}
.add_school .step{
    width:100%;
    height:80px;
    border-bottom:1px solid #ECEBE9;
}
.add_school .step .space,.school_area .space, .school_type .space ,.school_name .space, .smaster_tel .space, .your_tel .space,.apply_button .space{
    float:left;
    display:block;
    width:120px;
    height:100%;
    background:#F7F7F6;
    font-size:18px;
    color:#313030;
    font-weight:bold;
    text-align:center;
}
.space i {
    color:red;
    margin-right: 5px;
    position:relative;
    top:3px;
}
.add_school .school_area{
    width:100%;
    height:140px;
    line-height:140px;
    border-bottom:1px solid #ECEBE9;
}
.add_school .school_type,.school_name,.smaster_tel,.your_tel,.apply_button{
    width:100%;
    height:80px;
    line-height:80px;
    border-bottom:1px solid #ECEBE9;
}
.apply_school_btn,.apply_class_btn{
    float:right;
    width:174px;
    height:38px;
    line-height:38px;
    background:url('/apps/study/Public/Images/Home/btns.png') 0px 0px;
    margin-top:25px;
    margin-right:45px;
    border:none;
    color:#fff;
    font-size:14px;
    cursor:pointer;
}
.apply_school_btn:hover {
    background:url('/apps/study/Public/Images/Home/btns.png') 0px -38px;
}
.apply_class_btn:hover {
    background:url('/apps/study/Public/Images/Home/btns.png') 0px -38px;
}
.step *{
    line-height:80px;
}
.step ul li{
    float:left;
    width:220px;
    height:80px;
    text-align:center;
    font-size:18px;
    color:#999;
    overflow:hidden;
    cursor:pointer;
}
.step ul li.current{
    color:#F16662;
}
.step ul span{
    width:66px;
    text-align:center;
    color:#F99F99;
    font-size:20px;
    margin-left: 40px;
}
.step ul li cite{
    font-size:50px;
    margin-right:6px;
    font-family:'Alien League';
}
.school_area div{
    display:inline;
    width:820px;
    height:140px;
    float:left;
    padding:0 30px;
    overflow:hidden;
}
.show_message {
    float: left;
    line-height: 80px;
    font-size: 16px;
    color: red;
}
.school_area div .prompt{
    width:100%;
    height:60px;
    line-height:60px;
    border-bottom:1px dotted #8C8984;
    font-size:18px;
    color:#313030;
    font-weight:bold;
}
.school_area div .prompt cite{
    font-size:14px;
    color:#808080;
    font-weight:normal;
}
.school_area div div{
    float:left;
    width:440px;
    height:80px;
    line-height:80px;
    padding:0;
    font-size: 18px;
    color: #313030;
    font-weight: bold;
    _margin-top:50px;
}
.school_area div div select{
    margin:0 10px;
    color:##313030;
    font-size:14px;
    cursor:pointer;
}
.school_type div{
    float:left;
    padding-left:18px;
    height:80px;
    overflow:hidden;
}
.school_type div input{
    margin-right:5px;
    cursor:pointer;
}
.school_type div label{
    margin-right:42px;
    color:#606060;
    font-size:16px;
}
.school_name input, .smaster_tel input, .your_tel input{
    float:left;
    width: 363px;
    height: 39px;
    line-height: 39px;
    border: none;
    background: url('/apps/study/Public/Images/Home/cc_input.png') no-repeat;
    margin-top:20px;
    margin-left:15px;
    text-indent:5px;
}
.school_name span,.smaster_tel span,.your_tel span{
    margin-left:20px;
    color:red;
}


/* step2---选择班级 */
.step_by .filter{
    width:960px;
    height:80px;
    padding:0 20px;
    border-bottom:1px solid #ECEBE9;
}
.step_by .filter span{
    float:left;
    line-height:80px;
    font-size:16px;
    color: #313030;
}
.step_by .filter select{
    float:left;
    min-width:130px;
   _width:130px;
    margin-left:5px;
    margin-top:30px;
}
.step_by .filter .class_name{
    float:left;
    display:inline;
    height:80px;
    line-height:80px;
}
.filter .class_name input{
    height:20px;
}
.filter .add_btn{
    float:left;
    width:49px;
    height:25px;
    background:url('/apps/study/Public/Images/Home/add_class.png');
    margin-top: 27px;
    margin-left: 10px;
    cursor:pointer;
}
.step_by .filter .class_name label{
    float:left;
    color:red;
    margin-left:120px;
    font-size:16px;
    color: #313030;
    display:inline;
}
.filter_result{
    width:960px;
    height:600px;    /*tem*/
    padding:20px;
}
.filter_result ul{
    float:left;
    width:100%;
    margin-top:20px;
}
.filter_result ul li{
    float:left;
    width:960px;
    height:40px;
    line-height:40px;
}
.filter_result ul li.caption{
    font-size:14px;
}
.cap_num ,.cap_seg, .cap_grade, .cap_name, .cap_status ,.cap_operate{
    float: left;
    display: inline-block;
    text-align:center;
    color: #606060;
}
.filter_result ul li span.cap_num{
    width:10%;
}
.filter_result ul li span.cap_seg{
    width:15%;
}
.filter_result ul li span.cap_grade{
    width:15%;
}
.filter_result ul li span.cap_name{
    width:20%;
}
.filter_result ul li span.cap_status{
    width:15%;
}
.filter_result ul li span.cap_operate{
    width:25%;
    _width:24%;
}
.cap_operate i{
    cursor:pointer;
    color:#58C988;
}
.filter_result .reged{
    font-size: 16px;
    color: #313030;
}
.filter_result ul li.caption{
    border-top:1px solid #CDCDCD;
    background:#F2F2F2;
}

/* step3---添加人员 */
.reg_person p.school_class{
    float:left;
    width:930px;
    margin:0 35px;
    height:50px;
    line-height:50px;
    border-bottom:1px dotted #ECEBE9;
    text-align:center;
    font-size:18px;
    font-weight:bold;
    color:#333;
    display:inline;
}
.reg_person p.school_class cite{
    margin-left:30px;
}
.reg_person p.school_class a{
    font-size:12px;
    color:#F16662;
    margin-left:15px;
    font-weight:normal;
}
.add_teachers{
    float:left;
    width:950px;
    height:auto;
    margin:20px 25px;
    display:inline;
}
.add_teachers p.title, .add_students p.title{
    width:900px;
    height:40px;
    line-height:40px;
    background:#EEFFCC;
    border:1px solid #9BDE62;
    padding-left:30px;
    margin-left: 10px;
    font-size:16px;
    font-weight:bold;
    color:#666;
}
.add{
    width:570px;
    height:auto;
    margin:10px 0 10px 100px;
    border-right:1px dotted #9BDE62;
    display:inline;
}
.add p.top{
    float:left;
    padding:10px 0;
    display:inline;
}
.add p.top.left{
    width:200px;
    overflow:hidden;
    white-space:nowrap;
}
.add p.top.right{
    width:450px;
    overflow:hidden;
    white-space:nowrap;
}
.add p.top a{
    margin-left:15px;
    color:red;
}
.add ul li{
    float:left;
    width:570px;
    margin-bottom:15px;
}
.add ul li label{
    float:left;
    width: 200px;
}
.add ul li label em{
    float:left;
    width:33px;
}
.add ul li select{
    height:22px;
}
.add ul li a{
    margin-left:8px;
}
.add ul li a:hover{
    text-decoration: none;
    color: #F60;
}
.add ul li.myself span{
    margin-left:5px;
    margin-right:82px;
}
.add ul li .inp0_tj{
    color:#690;
    height:27px;
    line-height:27px;
    border:0 none;
    border-bottom:#CCC 1px solid;
    margin-right:10px;
    font-size: 14px;
}
.added{
    width:250px;
    height: auto;
    margin: 10px 0 10px 10px;
    display:inline;
}
.add_students{
    float:left;
    width:950px;
    height:auto;
    margin:20px 25px;
    display:inline;
}
.add_students .add{
    width:650px;
    height:auto;
    margin:10px 0 10px 20px;
    border-right:1px dotted #9BDE62;
}
.add_students .add .fastload{
    float:left;
    height:auto;
}
.manualload{
    float:left;
    width:450px;
}
.add_students .add ul{
    float:left;
    width:450px;
    height:auto;
}
.fastload textarea{
    float:left;
    width:110px;
    height:300px;
    border:#9C0 1px solid;
    padding:5px;
    line-height:18px;
    color:#666;
    resize: vertical;
    overflow-y: auto;
    outline-style: none;
}
.fastload a.importBtn{
    float:left;
    width:49px;
    height:35px;
    margin:130px 10px 0 10px;
    background:url('/apps/study/Public/Images/Home/import.png') no-repeat;
}
.add_students .add ul li{
    float: left;
    margin: 0px 0px 10px 3px;
    width: 130px;
    overflow: hidden;
    display: inline;
}
.add_students .add li .inp0_tj {
    width: 81px;
    margin-right: 5px;
}
.add_students .add li label em{
    float:left;
    width:33px;
    height:27px;
    line-height:25px;
    text-align:center;
    border-right:none;
}
.add_students .add .addStudent{
    float:left;
    width:300px;
    height: 25px;
    line-height: 25px;
    display: block;
    color: #999;
    margin-top:10px;
    margin-left:140px;
    display:inline;
}
.add_students .add .addStudent a{
    cursor:pointer;
}
.add_students .add .addStudent a{
    font-size:14px;
    font-weight:bold;
    color:#53C684;
}
.add_students .add .email{
    width:450px;
    margin-top:20px;
}
.add_students .add .email input{
    width:300px;
    color:#999;
}
.add_students .add .tjBtn{
    width: 174px;
    height:38px;
    line-height:38px;
    text-align:center;
    overflow: hidden;
    margin-top:20px;
    margin-left:100px;
    background:url('/apps/study/Public/Images/Home/btns.png') no-repeat 0px 0px;
    cursor:pointer;
    color:#fff;
    font-size:14px;
}
.add_students .add .tjBtn:hover{
    background:url('/apps/study/Public/Images/Home/btns.png') no-repeat 0px -38px;
}
