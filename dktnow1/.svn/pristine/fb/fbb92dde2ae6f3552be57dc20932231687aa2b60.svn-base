/* CSS Document */
* {
	margin:0;
	padding:0;
}
h1,h2,h3,h4{
    font-size:100%;
}
.clear{
    clear:both;
}
.fl{
    float:left;
}
.fr{
    float:right;
}
.mr10{
    margin-right:10px;
}
.mr5{
    margin-right:5px;
}
.ml5{
    margin-left:5px;
}
.mt5{
    margin-top:5px;
}
.mt10{
    margin-top:10px;
}
.ml10{
    margin-left:10px;
}
.pt10{
    padding-top:10px;
}
.pt5{
    padding-top:5px;
}
.pb5{
    padding-bottom:5px;
}
.dp{
    display:inline;
}
.dn{
    display:none;
}
.db{
    display:block;
}
.di{
    display:inline-block;
}
.wg{
    font-weight:bold;
}
.nw{
    font-weight:normal;
}
em, cite, i {
    font-style:normal;
}
ol,li{
    list-style:none;
}
a{
    color:#040404;
    text-decoration:none;
}