.mainBox {
    float:left;
    width:785px;
    height:auto;
    margin-top:25px;
}
.mainBox ul.mainBox_tab_menu {
    float:left;
    width:100%;
    height:auto;
    overflow: hidden;
}
.mainBox ul.mainBox_tab_menu li {
    float:left;
    width:150px;
    height:50px;
    line-height:50px;
    margin-right:20px;
    font-size:18px;
    color:#99999d;
    text-align: center;
    cursor:pointer;
}
.mainBox ul.mainBox_tab_menu li.selected {
    font-weight: 400;
    color:#080808;
}
.mainBox_tab_menu li.current {
    color:#080808;
    font-weight: 400;
    cursor:pointer;
}
.mainBox_tab_box > div {
    float: left;
    width:100%;
    background: #F3F2F0;
    box-shadow: 2px 2px 2px rgba(0, 0, 0, 0.1);
    overflow: hidden;
}
ul.course_list li.listli {
    float:left;
    width:235px;
    height:267px;
    box-shadow: 0 0 2px 2px rgba(0, 0, 0, 0.1);
