*{
    margin:0;
    padding:0;
}
body{
    font-family: "Hiragino Sans GB","Hiragino Sans GB W3","Microsoft YaHei","微软雅黑",tahoma,arial,simhei,"黑体";
    font-size:12px;
    background:url('/apps/study/Public/Images/Ilc/body_bg.png') repeat;
}
h1,h2,h3,h4{
    font-size:100%;
}
.clear{
    clear:both;
}
.fl{
    float:left;
}
.fr{
    float:right;
}
.mr10{
    margin-right:10px;
}
.mr5{
    margin-right:5px;
}
.ml5{
    margin-left:5px;
}
.mt5{
    margin-top:5px;
}
.mt10{
    margin-top:10px;
}
.ml10{
    margin-left:10px;
}
.pt10{
    padding-top:10px;
}
.pt5{
    padding-top:5px;
}
.pb5{
    padding-bottom:5px;
}
.dp{
    display:inline;
}
.dn{
    display:none;
}
.db{
    display:block;
}
.di{
    display:inline-block;
}
.wg{
    font-weight:bold;
}
.nw{
    font-weight:normal;
}
em, cite, i {
    font-style:normal;
}
ol,li{
    list-style:none;
}
a{
    color:#040404;
    text-decoration:none;
}
.hide{
    display:none;
}
img{
    border:0;
}

.warp{
    width:1000px;
    height:auto;
    margin:0 auto;
    padding-bottom:25px;
    position:relative;
}
.foot_bot,.foot_top{
    width:1000px;
    border-bottom:1px solid #b2b2b2;
    margin:0 auto;
    margin-top:10px;
    font-size:0px;
    line-height:0px;
}
.foot_top{
    border-bottom:1px solid #e8e8e8;
    margin-top:0px;
}
.class_edit {
    position:absolute;
    top:0;
    right:0;
    width: 18px;
    height: 17px;
    background: url("/apps/study/Public/Images/Ilc/write.png") no-repeat;
    display:none;
}
.manage_center{
    width:795px;
    margin-top:25px;
}
.coBtn .save ,.coBtn .reset{
    width:174px;
    height:38px;
    border:none;
    cursor:pointer;
    font-size:14px;
}
.coBtn .save {
    color:#fff;
    background:url('/apps/study/Public/Images/Ilc/btns.png') 0px 0px;
    margin-right:5px;
}
.coBtn .save:hover {
    background:url('/apps/study/Public/Images/Ilc/btns.png') 0px -38px;
}
.coBtn .reset {
    color:#292929;
    background:url('/apps/study/Public/Images/Ilc/btns.png') -174px 0px;
}
.coBtn .reset:hover {
    background:url('/apps/study/Public/Images/Ilc/btns.png') -174px -38px;
}
.page {
    float:right;
    margin-top:25px;
    margin-right:20px;
}
.page a {
    float:left;
    padding: 5px 10px;
    border: 1px solid #20D691;
    margin: 0px 2px;
    text-decoration: none;
}
.page a:hover {
    background:#93E5BD;
}

/* header */
#header{
    width:100%;
    height:67px;
    background:url('/apps/study/Public/Images/Ilc/headerBg.png');
}
#header div ul li a.on {
    background: none repeat scroll 0 0 #00944C;
}

#header div{
    width:1000px;
    margin:0 auto;
}
#logo{
    width:258px;
    height:48px;
    float:left;
    margin-top:9px;
    background:url('/apps/study/Public/Images/Home/logo.png') no-repeat;
}
#header div ul{
    float:left;
    width:440px;
    margin-left:50px;
}
#header div ul li{
    float:left;
    width:110px;
    height:67px;
    line-height:67px;
    text-align:center;
    overflow:hidden;
}
#header div ul li a{
    position:relative;
    display: block;
    width: 110px;
    height: 67px;
    font-size:16px;
    font-weight: bold;
    color:#fff;
    letter-spacing: 1px;
    cursor: pointer;
    overflow:hidden;
}
#header div ul li span{
    position:absolute;
    left:0;
    width:110px;
}
#header ul li a span.out{
    top:0px;
}
#header ul li a span.over{
    top:-67px;
    background:#00944C;
}
#header div .download{
    float:right;
    color:#fff;
    font-size:13px;
    margin-top:30px;
    cursor:pointer;
}
#header .exit {
    float: right;
    color: white;
    font-size: 12px;
    margin-top: 25px;
    margin-left:10px;
    cursor: pointer;
}
/* footer */
#footer{
    width: 100%;
    margin-top: 10px;
    padding:20px 0;
    text-align: center;
    color:#8D8A85;
}

/* leftSider */
#left_sider{
    float:left;
    width:215px;
    margin-top:25px;
}
#left_sider  div{
    float:left;
    width:210px;
    height:auto;
    margin-bottom:20px;
    background:#F3F2F0;
    box-shadow: 0 0 2px 2px rgba(0,0,0,0.1);
    /*box-shadow: inset 1px 1px 0 rgba(0, 0, 0, 0.1), inset 0 -1px 0 rgba(0, 0, 0, 0.07);*/
    /*-moz-box-shadow: 2px 2px 2px rgba(0,0,0,0.15);
    -webkit-box-shadow: 2px 2px 2px rgba(0,0,0,0.15);*/
}
#left_sider div p.title{
    width:210px;
    float:left;
    padding:15px 0;
    border-bottom:1px solid #E0DED9;
    color:#373838;
}
#left_sider div p.title img{
    margin-left:20px;
    vertical-align:middle;
}
#left_sider div p.title span{
    font-size:18px;
    margin-top:5px;
    vertical-align:middle;
}
#left_sider .main_user{
    height:130px;
    background:#F3F2F0;
    border-radius:5px 5px 0px 0px;
}
#left_sider .main_user img{
    float:left;
    margin-top:10px;
    margin-left:10px;
    
}
#left_sider .main_user .info{
    float:left;
    width:100px;
    margin-top:30px;
    background:none;
    box-shadow:none;
}
#left_sider .main_user .info a{
    float:left;
    margin-left:15px;
    width:80px;
}
#left_sider .main_user .info a.name{
    font-size:22px;
}
#left_sider .main_user .info a.university{
    font-size:12px;
    color:#2C6C82;
    margin-top:4px;
}
#left_sider .main_user .info a.mody_data{
    font-size:12px;
    color:#918E89;
    margin-top:8px;
}

#left_sider div ul{
    float:left;
    width:202px;
    margin:8px 0 0 8px;
    _margin:8px 0 10px 8px;
    padding-bottom:8px;
    display:inline;
}
#left_sider div ul li{
    float:left;
}
#left_sider div ul li a{
    float:left;
    font-size:12px;
}
#left_sider div ul li a.gname , #left_sider div ul li a.gtag{
    margin-left:10px;
    margin-top:2px;
    font-size: 12px;
}
#left_sider div ul li a.gname{
    color:#375E91;
    width: 130px;
}
#left_sider div ul li a.gtag{
    color:#918E89;
}
#left_sider div ul.group{
    width:170px;
    margin: 10px 0 0 20px;
    display:inline;
}
.main_app ul li a, .main_nav ul li a{
    width:76px;
    height:40px;
    line-height:40px;
    display:block;
    background:url('/apps/study/Public/Images/Ilc/left_btn.png') no-repeat;
    margin:8px 10px;
    text-align:center;
    color:#545450;
    display:inline;
}
.main_app ul li a:hover, .main_nav ul li a:hover{
    width:76px;
    height:40px;
    line-height:40px;
    display:block;
    background:url('/apps/study/Public/Images/Ilc/left_btn_h.png') no-repeat;
    margin:8px 10px;
    text-align:center;
    color:#545450;
    display:inline;
}

/*学生首页做部分*/
.main_app em{
    display:inline;
    float:left;
    width:30px;
    height:30px;
    margin:-8px 5px 0px 20px;
    overflow:hidden;
    background:url('/apps/study/Public/Images/Ilc/students_space.png') no-repeat -2px -0px;
}
.students_app span{
    line-height:25px;
    margin:10px 30px 0px 10px;
}
.students_app span a{
    color:#545450;
    font-size:14px;    
}
.main_app em.students_use{
    background-position:-28px 7px;
}
/*班级管理标准头部*/
.class_name{
    line-height:67px;
    color:#fff;
}
.class_head{
    width:30px;
    height:30px;
    margin:18px 0px 0px 7px;
    background:#999;
}
#header div #class_nav{
    width:550px;
}
.plist li{
    cursor:pointer;
}
/*showMessage*/
.showMessage_cover {
    background:black;
    /*opacity:0.2;
    filter: alpha(opacity = 20);*/
    position:absolute;
    left:0;
    top:0;
    background: rgba(0,0,0,0.2);
    overflow:hidden;
    z-index:200;
} 
.messageWin {
    border: 1px solid #d2d2dc;
    border-radius:5px;
    width:200px;
    height:88px;
    position : fixed;
    top:50%;
    left:50%;
    margin-top:-44px;
    margin-left:-100px;
    background:#f5f5f5;
    display:none;
    z-index:9999;
}
.exactMessage,.errorMessage {
    font-size:16px;
    text-align:center;
    margin-top:30px;
}
.exactMessage {
    color:blue;
}
.errorMessage {
    color:red;
}
/* 工具按钮 */
.filter .tools span,.xin_tools span{
    float:left;
    width:66px;
    height:28px;
    line-height:28px;
    background:url('/apps/study/Public/Images/Ilc/toolbg.png') no-repeat;
    margin-right:10px;
    color: #444;
    font-weight: 400;;
    cursor:pointer;
}
.filter .tools span.add i,.edit i,.del i,.sort i,.forbidden i,.resume i,.xin_tools span.add i{
    float:left;
    width:15px;
    height:15px;
    margin-top:6px;
    margin-left:8px;
    margin-right:5px;
}
.filter .tools span.add i ,.xin_tools span.add i{
    background:url('/apps/study/Public/Images/Ilc/plus.png') no-repeat;
}
.filter .tools span.edit i{
    background:url('/apps/study/Public/Images/Ilc/write.png') no-repeat;
}
.filter .tools span.del i{
    background:url('/apps/study/Public/Images/Ilc/plus.png') no-repeat;
}
.filter .tools span.sort i{
    background:url('/apps/study/Public/Images/Ilc/activity.png') no-repeat;
}
.filter .tools span.forbidden i{
    background:url('/apps/study/Public/Images/Ilc/locked.png') no-repeat;
}
.filter .tools span.resume i{
    background:url('/apps/study/Public/Images/Ilc/ok.png') no-repeat;
}
.filter .search {
    width:100%;
    height:auto;
    margin-top:12px;
}
.filter .search input {
    float:left;
    width: 313px;
    height: 39px;
    line-height: 39px;
    margin-right: 3px;
    border: none;
    background: url('/apps/study/Public/Images/Ilc/searchbg.png') no-repeat;
    text-indent:5px;
    color:#999;
    font-size:14px;
}
.filter .search .search_btn {
    float:left;
    width: 86px;
    height: 39px;
    background: url('/apps/study/Public/Images/Ilc/searchBtn.png') no-repeat;
    margin-left:6px;
}
.filter .search .folder {
    display:inline-block;
    margin-top:15px;
    margin-left:10px;
    cursor:pointer;
    font-size: 14px;
    /*background: url('/apps/study/Public/Images/Ilc/folderDown.png') no-repeat;*/
}
.filter .search .more_search {
    width:100%;
    height:auto;
}
.filter .search .more_search li {
    float:left;
    margin-bottom:10px;
}
.filter .search .more_search li.searchBtn{
    float:right;
}
.filter .search .more_search li label{
    width:80px;
    padding-right:10px;
    font-size: 14px;
    color: #494949;
    font-weight: 400;;
}
.filter .search .more_search li span{
    display: inline-block;
    margin: 0px 10px;
    padding: 5px 20px;
    border: 1px solid #20D691;
    cursor: pointer;
}
.filter .search .more_search li.on {
    background: #93E5BD;
}
.filter .search .more_search li.gcType {
    float:left;
    margin-top: 10px;
}
.filter .search .more_search li.searchBtn a {
    float:right;
    margin-top:10px;
}
.filter .search .more_search li div {
    float: left;
    width: 680px;
}
/* 表格排列 */
._caption {
    float:left;
    width: 800px;
    height: 60px;
    padding-left: 20px;
    background: #26A75B;
    filter: alpha(opacity=70);
    opacity: 0.7;
}
._caption li {
    float: left;
    height: 60px;
    line-height: 60px;
    text-align: center;
    color: white;
    cursor:pointer;
    font-size: 14px;
}
.t_caption {
    float:left;
    width: 800px;
    height: 60px;
    padding-left: 20px;
    background: #26A75B;
    filter: alpha(opacity=70);
    opacity: 0.7;
    overflow:hidden;
}
.t_caption li,.plist li {
    float: left;
    height: 60px;
    line-height: 60px;
    text-align: center;
    color: white;
    cursor:pointer;
    font-size: 14px;
}
.plist {
    float:left;
    width: 800px;
    height: 60px;
    padding-left: 20px;
    background: #F2F2F2;
    border-bottom: 1px solid #E1DEDA;
    filter: alpha(opacity=70);
    opacity: 0.7;
    overflow:hidden;
}
.plist ul {
    float:left;
    width:800px;
}
.plist li {
    color:#424242;
    overflow:hidden;
}
#data {
    display:inline;
    color:red;
}
._List {
    background: #F2F2F2;
    border-bottom: 1px solid #E1DEDA;
}
._List ul li {
    color:#424242;
    overflow:hidden;
}

