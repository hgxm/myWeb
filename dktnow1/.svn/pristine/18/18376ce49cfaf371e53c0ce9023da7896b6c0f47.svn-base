#pragma mark requestForClasshour
-(void)mySuccessForClassHour:(id)sender{
    
    if (classHourList) {
        [classHourList removeAllObjects];
        [classHourList release];
        classHourList=nil;
    }
    
    
    NSString *str=[sender responseString];
    NSMutableDictionary *muDict = [str JSONValue];
    NSLog(@"mudict is %@",muDict);
    if ([[muDict objectForKey:@"status"] intValue]!=0) {
        classHourList = [[[muDict objectForKey:@"info"] objectForKey:@"list"] mutableCopy];
        [self loadHourScrollview:classHourList];

    }
  
   
}


