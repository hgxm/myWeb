/* 进入课程 */
/* CSS Document */
.lesson_left {
    width:191px;
    min-height:800px;
    _height:800px;
    padding:15px 0px 0px 17px;
    margin-top:25px;
    background:#F3F2F0;
    box-shadow: 0 0 2px 2px rgba(0, 0, 0, 0.1);
    overflow:hidden;
}
.left_top {
    width:110px;
    height:38px;
    line-height:38px;
    padding-left:64px;
    background:url('../Images/classroom.png') no-repeat 0px -38px;
    color:#ffffff;
    font-size:14px;
    cursor:pointer;
}
.lesson_left table {
    margin-top:10px;
    height:auto;
}
.lesson_left table tr td {
    width:260px;
}
.lesson_left table tr td ul {
    overflow:auto;
}
#tree > li > a {
    color: #818181;
    font-size:18px;
    font-weight: 400;;
}
/*右部分*/
.lesson_righ {
    width:766px;
    padding-top:38px;
    margin-left:20px;
    margin-bottom:20px;
    display:inline;
}
.lesson_revert, .lesson_back {
    padding:12px 21px 0px 9px;
    color:#080808;
    font-size:18px;
}
.lesson_back{
    margin-right:9px;
}
.lesson_back:hover {
    color:#22a558;
}
.lesson_revert:hover {
    color:#22a558;
}
.lesson_revert {
    color:#3aba6e;
    display:block;
    width:640px;
    overflow:hidden;
    white-space:nowrap;
    /*text-overflow:ellipsis;*/
}
.lesson_dir  {
    font-size: 20px;
    line-height:45px;
}
.lesson_mark {
    padding-top:12px;
}
.lesson_lead {
    float:right;
    display: block;
    width: 124px;
    height: 40px;
    line-height: 40px;
    margin-top: 5px;
    margin-right: 10px;
    border: 1px solid #E6E6E6;
    color: #555;
    font-size: 14px;
    text-align: center;
}
.lesson_tab {
    position:relative;
    float:left;
    width:100%;
}
.lesson_tab li {
    float:left;
    display:block;
    position:relative;
    height:60px;
    line-height:60px;
    margin:0px 35px 0px 8px;
    color:#99999d;
    font-size:18px;
    cursor:pointer;
}
.lesson_tab li.selected_green {
    color:#080808;
    font-weight: 400;;
    background: url('../Images/bk_selected_green.png') no-repeat bottom;
}
.lesson_tab li.selected_gray {
    color:#080808;
    font-weight: 400;;
    background: url('../Images/bk_selected_gray.png') no-repeat bottom;
}
.lesson_norm {
    display:none;
    position:absolute;
    left:45px;
    top:45px;
    width:11px;
    height:5px;
    background:url('../Images/lesson.png') no-repeat -30px -36px;
}
.lesson_add {
    width:123px;
    height:40px;
    line-height:40px;
    border:1px solid #E8E8E4;
    color:#555555;
    font-size:14px;
