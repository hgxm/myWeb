/* 我的网盘 */
.resbox {
    float:left;
    width:1024px;
    height:auto;
    border:1px solid #cecece;
    background:#fff;
}
.resbox .modl_tab {
    float:left;
    width:100%;
}
.resbox .modl_tab li.menu {
    float:left;
    width:125px;
    height:45px;
    line-height:45px;
    font-size:14px;
    color:#222;
    text-align:center;
    border-right:1px solid #D1CEC9;
    background:url('/apps/nr/Public/Images/Home/modl_tab_bg.png');
    cursor:pointer;
}
.resbox .modl_tab .menu.current {
    background:url('/apps/nr/Public/Images/Home/modl_tab_on.png');
    font-weight:bold;
    color:#0482f1;
    position:relative;
    top:1px;
}
.res_show_area {
    float:left;
    width:100%;
    border-top:1px solid #D1CEC9;
    margin-bottom:40px;
}
.resbox .modl_tab li.upload {
    float:right;
    width:112px;
    height:37px;
    background:url('/apps/nr/Public/Images/Home/upload_btn.png') 0px 0px;
    margin-top:4px;
    margin-right:20px;
    cursor:pointer;
    position:relative;
}
.resbox .modl_tab li.upload:hover {
    background:url('/apps/nr/Public/Images/Home/upload_btn.png') 0px -37px;
}
.res_show .rearch_box {
    float:left;
    position:relative;
    padding-left:30px;
    padding-top:20px;
    display:inline;
}
.res_show .rearch_box .sch_input {
    width:216px;
    height:30px;
    background:url('/apps/nr/Public/Images/Home/sch_input_bg.png');
    border:none;
    padding-left:10px;
}
.res_show .rearch_box .sch_btn {
    width:50px;
    height:30px;
    background:url('/apps/nr/Public/Images/Home/sch_btn.png');
    border:none;
    position:absolute;
    left:206px;
    top:20px;
    cursor:pointer;
}
.res_type {
    float:left;
    display:inline;
}
.res_show .res_type_tab {
    float:left;
    width:964px;
    margin:20px 30px 0;
    font-size:14px;
    color:#6a6a6a;
    position:relative;
    display:inline;
}
.res_show .res_type_tab li {
    float:left;
    margin-right:30px;
    cursor:pointer;
}
.res_show .res_type_tab li.on {
    color:#dc6106;
}
.res_type .tools_box {
    float: left;
    width:994px;
    padding: 20px 0 0 30px;
}
.res_type .tools_box .choose {
    float:left;
    line-height:28px;
    font-size:14px;
}
.res_type .tools_box .choose input {
    margin-right: 10px;
    position: relative;
    top: 2px;
}
.res_type .tools_box .choose span {
    color:#6a6a6a;
}
.res_type .tools_box .choose span i {
    color:#F44D4B;
}
.tools_box .edit ,.tools_box .publish , .tools_box .download ,.tools_box .share ,.tools_box .del{
    float:left;
    width: 61px;
    height: 27px;
    margin-left: 15px;
    cursor: pointer;
}
.tools_box .edit {
    background: url('/apps/nr/Public/Images/Home/res_tools.png') 0px 0px;
}
.tools_box .edit:hover {
    background: url('/apps/nr/Public/Images/Home/res_tools.png') -61px 0px;
}
.tools_box .publish {
    background: url('/apps/nr/Public/Images/Home/res_tools.png') 0px -27px;
}
.tools_box .publish:hover {
    background: url('/apps/nr/Public/Images/Home/res_tools.png') -61px -27px;
}
.tools_box .download {
    background: url('/apps/nr/Public/Images/Home/res_tools.png') 0px -54px;
}
.tools_box .download:hover {
    background: url('/apps/nr/Public/Images/Home/res_tools.png') -61px -54px;
}
.tools_box .share {
    background: url('/apps/nr/Public/Images/Home/res_tools.png') 0px -81px;
}
.tools_box .share:hover {
    background: url('/apps/nr/Public/Images/Home/res_tools.png') -61px -81px;
}
.tools_box .del {
    background: url('/apps/nr/Public/Images/Home/res_tools.png') 0px -108px;
}
.tools_box .del:hover {
    background: url('/apps/nr/Public/Images/Home/res_tools.png') -61px -108px;
}
.tools_box .tool_manage .manage {
    float:left;
    width: 61px;
    height: 27px;
    margin-left: 15px;
    cursor: pointer;
    background: url('/apps/nr/Public/Images/Home/res_tools_show.png') 0px 0px;
}
.tools_box .tool_manage .manage:hover {
    background: url('/apps/nr/Public/Images/Home/res_tools_show.png') -61px 0px;
}
.order_switch {
    float: right;
    width: 64px;
    margin-right: 32px;
}
.order_switch .list, .order_switch .thumb {
    float: left;
    width: 32px;
    height: 28px;
}
.order_switch .list {
    background: url('/apps/nr/Public/Images/Home/order_switch.png') 0px 0px;
}
.order_switch .list.on,.order_switch .list:hover {
    background: url('/apps/nr/Public/Images/Home/order_switch.png') 0px -28px;
}
.order_switch .thumb {
    background: url('/apps/nr/Public/Images/Home/order_switch.png') -31px -28px;
}
.order_switch .thumb.on ,.order_switch .thumb:hover {
    background: url('/apps/nr/Public/Images/Home/order_switch.png') -31px 0px;
}
.res_show .reslist_box {
    float:left;
    width:994px;
    margin-top:10px;
    margin-left:30px;
    display:inline;
}
.res_show .reslist_box li {
    float: left;
    width: 124px;
    height: 114px;
    background: url('/apps/nr/Public/Images/Home/rebox_bg_s.png');
    margin: 12px 16px 0 0;
    position:relative;
}
.reslist_box li .licover {
    position:absolute;
    top:6px;
    left:12px;
    width:100px;
    height:75px;
    background:url('/apps/nr/Public/Images/Home/licover.png');
    display:none;
}
.reslist_box li .licover .edit_cover {
    width:18px;
    height:15px;
    position:absolute;
    top:5px;
    right:25px;
    background:url('/apps/nr/Public/Images/Home/edit_cover.png') no-repeat;
}
.reslist_box li .licover .del_cover {
    width:13px;
    height:14px;
    position:absolute;
    top:5px;
    right:5px;
    background:url('/apps/nr/Public/Images/Home/del_cover.png') no-repeat;
}
.res_show .reslist_box li .res_cover {
    float:left;
    position: relative;
}
.res_show .reslist_box li .res_cover .old_frame {
    float: left;
    height: 75px;
    margin: 6px 0 0 12px;
    cursor: pointer;
}
.res_show .reslist_box li .res_title {
    float: left;
    width: 100px;
    margin-top: 3px;
    margin-left: 12px;
    cursor: pointer;
    color: #252525;
    font-size: 13px;
    white-space: nowrap;
    overflow: hidden;
}
.reslist_box li .res_cover .res_click {
    position: absolute;
    top: 3px;
    left: 3px;
}
.reslist_box li .res_cover .res_scan {
    position:absolute;
    top:3px;
    left:3px;
}
.reslist_box li.click .res_click {
    display:inline;
}
.reslist_box li.on .res_scan{
    display:inline;
}
.reslist_box li.listli {
    width: 960px;
    height: 50px;
    line-height: 50px;
    border-bottom: 1px solid #DFDCD8;
    margin-top: 0;
    background:none;
}
.reslist_box li.listli .time {
    float: right;
    color: #A09C96;
    margin-right: 10px;
    display:inline;
}
.reslist_box li.listli .ck {
    float: left;
    margin-top: 20px;
    display:inline;
}
.reslist_box li.listli .res_cover{
    display:none;
}
.res_show .reslist_box li.listli .res_title {
    width:600px;
}

/* 我的收藏 */
.res_show .collect_num {
    position:absolute;
    right:0;
    background:url('/apps/nr/Public/Images/Home/coll_mark.png') no-repeat 0 center;
    text-indent:1.2em;
}
/*没有上传*/
.no-collect {
    width:386px;
    height:58px;
    background:url('/apps/nr/Public/Images/Home/no-collect.png') no-repeat;
    margin:200px auto;
}
/*没有收藏*/
.no-res{
    width:386px;
    height:58px;
    background:url('/apps/nr/Public/Images/Home/no-res.png') no-repeat;
    margin:200px auto;
}
/*上传资源*/
.upload_area {
    float:left;
    width:1024px;
    height:450px;
    border:1px solid #cecece;
    overflow:hidden;
}
.uplod_top {
    float:left;
    width: 100%;
    height: 45px;
    line-height: 45px;
    background: url('/apps/nr/Public/Images/Home/modl_tab_bg.png');
    border-bottom:1px solid #cecece;
}
.upload_area .upload_res {
    float:right;
    width:112px;
    height:37px;
    margin-top:4px;
    margin-right:20px;
    background:url('/apps/nr/Public/Images/Home/upload_btn.png') 0px 0px;
    cursor:pointer;
}
.upload_area .upload_res:hover {
    background:url('/apps/nr/Public/Images/Home/upload_btn.png') 0px -37px;
}
.upload_area .upload_box {
    float:left;
    width:100%;
    margin-top:20px;
    margin-left:20px;
}
#uploader {
    float: left;
    display: inline-block;
    width: 615px;
    margin-top: 15px;
    margin-left: 30px;
}
.upload_box .warn_word {
    float: left;
    width: 200px;
    line-height: 25px;
    margin-top: 90px;
    margin-left: 50px;
    font-size: 12px;
    color: #f76060;
}
.upload_box .finish {
    width:515px;
    margin:30px auto;
}
.upload_box .finish .finish_btn {
    width:123px;
    height:37px;
    background:url('/apps/nr/Public/Images/Home/upload_res_btn.png');
    border:0;
    /*margin-left:80px;*/
}