/*右侧头部*/
.top_tab_menu{
    float: left;
    width: 750px;
    margin-top: 25px;
    margin-left:30px;
}
.top_tab_menu li{
    display:inline;
    float: left;
    max-width:155px;
    height: 50px;
    line-height: 50px;
    margin-right: 20px;
    padding:0px 6px;
    font-size: 18px;
    color: #99999D;
    text-align: center;
    overflow:hidden;
    cursor: pointer;
}
.top_tab_menu li.selected,.top_tab_menu li.tab_return, .top_tab_menu li.tab_name{
    font-weight:bold;
    color:#080808;
}
.top_tab_menu li.selected{
    background: url('/apps/study/Public/Images/Home/d_selected.png') no-repeat bottom;
}
.top_tab_menu li.tab_return{
    float:right;
}
.top_tab_menu li.tab_return:hover {
    color:#22a558;
}
.top_tab_menu li.selected, .top_tab_menu li.tab_return, .top_tab_menu li.tab_name{
    font-weight:400;
}
.top_tab_menu li.tab_return,{
    margin-right:30px;
