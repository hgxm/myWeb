    width:60px;
    background:none;
    font-size:14px;
    text-align:center;
    overflow:hidden;
}
.rig_edit label.edit_leaguer{
    float:left;
    display:block;
    width:16px;
    height:15px;
    margin:12px 0px 0px 12px;
